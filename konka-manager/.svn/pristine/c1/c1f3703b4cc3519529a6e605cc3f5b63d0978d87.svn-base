<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %><%@ include file="/commons/pages/taglibs.jsp" %>
<table> 
<tr>
<td width="5%" nowrap="nowrap" align="center">序号</td><td nowrap="nowrap" align="center">交易流水号</td><td nowrap="nowrap" width="7%">正在处理的部门</td><td nowrap="nowrap" width="10%">下单时间</td><td width="10%" nowrap="nowrap" align="center">下单人姓名</td><td width="10%" nowrap="nowrap" align="center">购买人姓名</td><td width="15%" nowrap="nowrap" align="center">购买人地区</td><td width="10%" nowrap="nowrap" align="center">购买人手机号码</td><td width="10%" nowrap="nowrap" align="center">商品</td><td width="10%" nowrap="nowrap" align="center">兑换积分</td><td width="10%" nowrap="nowrap" align="center">配送方式</td><td nowrap="nowrap"width="7%" >备注</td><td nowrap="nowrap"width="7%" >订单状态</td>
</tr><c:forEach var="cur" items="${entityList}" varStatus="vs">
<tr><td height="28" align="center">${vs.count}</td><td align="left" style="mso-number-format:'\@';"> <c:out value="${cur.trade_index}" /></td><td align="left" valign="middle"> <c:out value="${cur.map.dept_name}" /></td><td align="left" valign="middle"><fmt:formatDate value="${cur.add_date}" pattern="yyyy-MM-dd HH:mm:ss" /></td><td align="left"><c:out value="${cur.order_user_name}" /></td><td align="left"><c:out value="${cur.buyer_name}" /></td><td align="left"><c:out value="${cur.map.full_name}" /> ${fn:escapeXml(cur.buyer_addr)}</td><td align="center" nowrap="nowrap"><c:out value="${cur.buyer_mp}" /></td><td align="center" nowrap="nowrap">${fn:escapeXml(cur.ecGift.pd_sn)}<br/> ${fn:escapeXml(cur.ecGift.pd_name)}</td><td align="center" nowrap="nowrap">${fn:escapeXml(cur.integral)}</td><td align="center"><c:if test="${cur.deliver_way eq '0'}">上门自提</c:if><c:if test="${cur.deliver_way eq 1}">商家配送</c:if></td><td align="left">${cur.remark }</td><td align="left"><c:if test="${cur.state eq -30 }">已退货</c:if><c:if test="${cur.state eq -20 }">审核未通过</c:if><c:if test="${cur.state eq -10 }">已关闭</c:if><c:if test="${cur.state eq 0 }">已预订</c:if><c:if test="${cur.state eq 10 }">已兑换</c:if><c:if test="${cur.state eq 20 }">审核通过</c:if><c:if test="${cur.state eq 30 }">下发处理</c:if><c:if test="${cur.state eq 40 }">商家发货</c:if><c:if test="${cur.state eq 50 }">客户已换货</c:if><c:if test="${cur.state eq 60 }">确认收货</c:if></td> 
</tr></c:forEach>
</table>