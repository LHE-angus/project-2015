/*CSS reset*/
body {
	font:12px/20px Microsoft YAHEI,"宋体","黑体",sans-serif;
	color:#333;
	margin:0;
	background-repeat: repeat;
}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td {

	
	
}
fieldset,img {border:0;}
address,caption,cite,code,dfn,em,strong,th,var {font-style:normal;font-weight:normal;}
ol,ul {
	list-style:none;
	line-height: normal;
	font-weight: bold;
	float: right;
	margin: 0px;
}
caption,th {text-align:left;}
h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:normal;
	text-align: center;
}
form {margin:0;padding:0;}
q:before,q:after {content:"";}
abbr,acronym {border:0;}
img,a img {vertical-align:middle;border:0;}
a:link, a:visited {color:#000;text-decoration:none;}
a:hover {color:#dd0000;text-decoration:underline;}
a:active {color:#000;text-decoration:none;}
input,textarea {font-size:12px;}

.swepimg{ cursor:pointer;}
table.tdTab{
	border-collapse:collapse;
}
table.tdTab td{
	line-height: 25px;
	font-size: 14px;
	color: #666;
}
.webinput {
	font-family:Microsoft YAHEI, '宋体';
	border:1px solid #7f9db9;
	color:#515151;
	line-height:26px;
	padding-left:5px;
	height:26px;
	width: 260px;
}
.websub{
	padding:0 5px 2px 5px !important;
	padding:0 2px;
	color:#fff;
	height:42px;
	line-height:40px;
	border:solid 1px #fff;
	background-image: url(../images/02.jpg);
	background-repeat: repeat-x;
	background-position: 0 0;
	width: 266px;
	font-family: Microsoft YAHEI,"宋体";
	font-size: 16px;
	font-weight: bold;
}

/* position*/
.tl {text-align:left;}
.tc {text-align:center;}
.tr {text-align:right;padding-right: 1px;}
.bc {margin-left:auto;margin-right:auto;}
.l {float:left;display:inline;}
.r {float:right;display:inline;}
.clear {clear:both;height:0;overflow:hidden;}
.clearfix:after {clear:both;height:0;overflow:hidden;display:block;visibility:hidden;content:".";}
.vm {vertical-align:middle;}
.pr {position:relative;}
.pa {position:absolute;}
.zoom {zoom:1;}
.hidden {visibility:hidden;}
.none {}
.block {display:block;}
.inline {display:inline;}
.hand {cursor:pointer;}
strong {font-weight:bold;}
.bg_eee{background-color:#eee;}
/* font */
.orgf {color:#ff4e00;}
.redfont,.redfont a,a.redfont { color: #f00!important; float: right;margin-top:10px}
.redfont1,.redfont1 a,a.redfont1 {
	color: #003!important;
	font-size: 12px;
}

.grayfont,.grayfont a.a.grayfont { color: #999!important; }
.bluefont,.bluefont a,a.bluefont{ color:#185fbe!important;}
.tdTab tr td a{
	color:#900;
	font-size:12px;
}
.tdTab tr td a,.tdTab tr td a:visited{ color:#333; text-decoration:none; font-size: 12px;}
.tdTab tr td a:hover{
	color:#900;
	text-decoration:underline;
	font-size: 12px;
	float: right;
	margin-top: 10px;
}

