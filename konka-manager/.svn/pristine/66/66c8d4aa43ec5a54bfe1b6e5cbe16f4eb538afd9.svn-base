	<select id="selectKonkaParagonAttentionCPaginatedList" resultMap="konkaParagonAttentionCResult" parameterClass="konkaParagonAttentionC" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		SELECT A.*,
