		  FROM KONKA_PARAGON_ATTENTION_C A
		  LEFT JOIN KONKA_PARAGON_SHOWINFO D ON A.SHOW_SHOP_CODE = D.SHOW_SHOP_CODE
		  LEFT JOIN KONKA_DEPT B ON B.DEPT_ID = D.PART_COMPANY_ID
		                        AND B.DEPT_TYPE = 3
		  LEFT JOIN KONKA_DEPT C ON C.DEPT_ID = D.CHANNEL_ID
		                        AND C.DEPT_TYPE IN (4, 5)
		<include refid="sf-konkaParagonAttentionC" />
