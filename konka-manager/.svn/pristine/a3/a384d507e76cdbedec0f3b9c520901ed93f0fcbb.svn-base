<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="EC_GIFT">

	<typeAlias alias="ecGift" type="com.ebiz.mmt.domain.EcGift" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertEcGift" />
		<flushOnExecute statement="updateEcGift" />
		<flushOnExecute statement="deleteEcGift" />
	</cacheModel>

	<resultMap id="ecGiftResultForList" class="ecGift">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="DEPT_SN" property="dept_sn" jdbcType="VARCHAR" />
		<result column="PD_NAME" property="pd_name" jdbcType="VARCHAR" />
		<result column="PD_SN" property="pd_sn" jdbcType="VARCHAR" />
		<result column="OWN_SYS" property="own_sys" jdbcType="DECIMAL" />
		<result column="NEED_INTEGRAL" property="need_integral" jdbcType="DECIMAL" />
		<result column="MAIN_PIC" property="main_pic" jdbcType="VARCHAR" />
		<result column="DOWN_TIME" property="down_time" jdbcType="TIMESTAMP" />
		<result column="LAST_UP_TIME" property="last_up_time" jdbcType="TIMESTAMP" />
		<result column="VIEW_COUNTS" property="view_counts" jdbcType="DECIMAL" />
		<result column="ORDER_VALUE" property="order_value" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="TIMESTAMP" />
		<result column="STATE" property="state" jdbcType="DECIMAL" />
		<result column="ADD_U_ID" property="add_u_id" jdbcType="DECIMAL" />
		<result column="SALE_NUM" property="sale_num" jdbcType="DECIMAL" />
		<result column="ORIGINAL_PRICE" property="original_price" jdbcType="DECIMAL" />
		<result column="ID" property="ecGiftComm" select="selectEcGiftCommByGiftId" />		
	</resultMap>

	<resultMap id="ecGiftResult" class="ecGift" extends="ecGiftResultForList">
	</resultMap>

	<sql id="sf-ecGift">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_sn">DEPT_SN = #dept_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_name">PD_NAME = #pd_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_sn">PD_SN = #pd_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="own_sys">OWN_SYS = #own_sys:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="need_integral">NEED_INTEGRAL = #need_integral:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="main_pic">MAIN_PIC = #main_pic:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="down_time">DOWN_TIME = #down_time:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="last_up_time">LAST_UP_TIME = #last_up_time:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="view_counts">VIEW_COUNTS = #view_counts:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="state">STATE = #state:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_u_id">ADD_U_ID = #add_u_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="sale_num">SALE_NUM = #sale_num:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.pd_sn_like">PD_SN like '%' || #map.pd_sn_like# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.pd_name_like">PD_NAME like '%' || #map.pd_name_like# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.is_upself">DOWN_TIME > sysdate</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.pd_sn_or_pd_name_like">PD_SN like '%' || #map.pd_sn_or_pd_name_like# || '%' or PD_NAME like '%' || #map.pd_sn_or_pd_name_like# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.need_integral_lt"><![CDATA[ NEED_INTEGRAL < #map.need_integral_lt# ]]></isNotEmpty>
		<isNotEmpty prepend=" and " property="map.need_integral_ge"><![CDATA[ NEED_INTEGRAL >= #map.need_integral_ge# ]]></isNotEmpty>
	</sql>

	<select id="selectEcGift" resultMap="ecGiftResult" parameterClass="ecGift" cacheModel="oneDayCache">
		select * from EC_GIFT where 1 = 1
		<include refid="sf-ecGift" />
	</select>

	<select id="selectEcGiftList" resultMap="ecGiftResultForList" parameterClass="ecGift" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from EC_GIFT where 1 = 1
		<include refid="sf-ecGift" />
		<isEmpty prepend=" " property="map.order_by_sale_num_desc"> order by order_value ,id desc </isEmpty>
		<isNotEmpty prepend=" " property="map.order_by_sale_num_desc"> order by sale_num desc, order_value desc </isNotEmpty>
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectEcGiftCount" resultClass="long" parameterClass="ecGift" cacheModel="oneDayCache">
		select count(*) from EC_GIFT where 1 = 1
		<include refid="sf-ecGift" />
	</select>

	<select id="selectEcGiftPaginatedList" resultMap="ecGiftResult" parameterClass="ecGift" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from EC_GIFT where 1 = 1
		<include refid="sf-ecGift" />
		 order by order_value desc,id desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>	
	
	<!-- add by tudp 2013-08-15 -->
	<select id="selectEcGiftWithId" resultMap="ecGiftResult" parameterClass="long">
		select * from EC_GIFT where ID = #value#
	</select>
	
	<insert id="insertEcGift" parameterClass="ecGift">
		<selectKey resultClass="long" keyProperty="id">select PSHOW_BASE_SEQ.nextval as id from dual </selectKey>
		<![CDATA[insert into EC_GIFT (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="dept_sn">DEPT_SN</isNotNull>	
			<isNotNull prepend="," property="pd_name">PD_NAME</isNotNull>	
			<isNotNull prepend="," property="pd_sn">PD_SN</isNotNull>	
			<isNotNull prepend="," property="own_sys">OWN_SYS</isNotNull>	
			<isNotNull prepend="," property="need_integral">NEED_INTEGRAL</isNotNull>	
			<isNotNull prepend="," property="main_pic">MAIN_PIC</isNotNull>	
			<isNotNull prepend="," property="down_time">DOWN_TIME</isNotNull>	
			<isNotNull prepend="," property="last_up_time">LAST_UP_TIME</isNotNull>	
			<isNotNull prepend="," property="view_counts">VIEW_COUNTS</isNotNull>	
			<isNotNull prepend="," property="order_value">ORDER_VALUE</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="state">STATE</isNotNull>	
			<isNotNull prepend="," property="add_u_id">ADD_U_ID</isNotNull>	
			<isNotNull prepend="," property="sale_num">SALE_NUM</isNotNull>	
			<isNotNull prepend="," property="original_price">ORIGINAL_PRICE</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_sn">#dept_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_name">#pd_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_sn">#pd_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="own_sys">#own_sys:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="need_integral">#need_integral:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="main_pic">#main_pic:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="down_time">#down_time:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="last_up_time">#last_up_time:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="view_counts">#view_counts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">#order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="state">#state:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_u_id">#add_u_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="sale_num">#sale_num:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="original_price">#original_price:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateEcGift" parameterClass="EcGift">
		update EC_GIFT
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_sn">DEPT_SN = #dept_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_name">PD_NAME = #pd_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_sn">PD_SN = #pd_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="own_sys">OWN_SYS = #own_sys:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="need_integral">NEED_INTEGRAL = #need_integral:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="main_pic">MAIN_PIC = #main_pic:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="down_time">DOWN_TIME = #down_time:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="last_up_time">LAST_UP_TIME = #last_up_time:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="view_counts">VIEW_COUNTS = #view_counts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="state">STATE = #state:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_u_id">ADD_U_ID = #add_u_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="sale_num">SALE_NUM = #sale_num:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="original_price">ORIGINAL_PRICE = #original_price:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteEcGift" parameterClass="EcGift">
		delete from EC_GIFT where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>