package com.ebiz.mmt.domain;

import java.io.Serializable;
import java.util.Date;

import com.ebiz.ssi.domain.BaseDomain;


public class KonkaJobs extends BaseDomain implements Serializable {

	private static final long serialVersionUID = -1L;

	private String fun_name;
	
	private String job_desc;
	
	private Integer status;
	
	private String excut_time;
	
	private Date update_time;
	
	private String updated_name;
	
	private Long updated_id;
	
	public KonkaJobs() {

	}

	/**
	 * @val 执行任务方法名称
	 */
	public String getFun_name() {
		return fun_name;
	}
	
	/**
	 * @val 执行任务方法名称
	 */
	public void setFun_name(String fun_name) {
		this.fun_name = fun_name;
	}
	
	/**
	 * @val 任务描述
	 */
	public String getJob_desc() {
		return job_desc;
	}
	
	/**
	 * @val 任务描述
	 */
	public void setJob_desc(String job_desc) {
		this.job_desc = job_desc;
	}
	
	/**
	 * @val 任务状态：0-启用 1-未启用
	 */
	public Integer getStatus() {
		return status;
	}
	
	/**
	 * @val 任务状态：0-启用 1-未启用
	 */
	public void setStatus(Integer status) {
		this.status = status;
	}
	
	/**
	 * @val 执行时间点，频率等描述
	 */
	public String getExcut_time() {
		return excut_time;
	}
	
	/**
	 * @val 执行时间点，频率等描述
	 */
	public void setExcut_time(String excut_time) {
		this.excut_time = excut_time;
	}
	
	/**
	 * @val 更新时间
	 */
	public Date getUpdate_time() {
		return update_time;
	}
	
	/**
	 * @val 更新时间
	 */
	public void setUpdate_time(Date update_time) {
		this.update_time = update_time;
	}
	
	/**
	 * @val 更新人姓名
	 */
	public String getUpdated_name() {
		return updated_name;
	}
	
	/**
	 * @val 更新人姓名
	 */
	public void setUpdated_name(String updated_name) {
		this.updated_name = updated_name;
	}
	
	/**
	 * @val 更新人ID
	 */
	public Long getUpdated_id() {
		return updated_id;
	}
	
	/**
	 * @val 更新人ID
	 */
	public void setUpdated_id(Long updated_id) {
		this.updated_id = updated_id;
	}
	
}