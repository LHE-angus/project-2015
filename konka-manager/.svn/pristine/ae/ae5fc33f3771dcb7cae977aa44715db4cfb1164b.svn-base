	<!-- Add by XingXiuDong @20110517 -->
	<select id="selectJxcBrandApplyXCount" resultClass="long" parameterClass="jxcBrandApply" >
		select count(*)
		  from (select t.brand_name,
		               max(t.add_date) last_add_date,
		               count(1) counts,
		               listagg(t.shop_name, ',') shop_names
		          from (select t1.*, t2.shop_name
		                  from JXC_BRAND_APPLY t1, entp_shop t2
		                 where t1.shop_id = t2.shop_id
		                 order by t2.shop_name) t
		         group by t.brand_name) t3
		  left join base_brand_info t4 on t3.brand_name = t4.brand_name
	</select>
	
	<!-- Add by XingXiuDong @20110517 -->
	<select id="selectJxcBrandApplyXPaginatedList" resultMap="jxcBrandApplyResultForXList" parameterClass="jxcBrandApply" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		  select t3.brand_name,
	       t3.last_add_date as add_date,
	       t3.counts,
	       to_char(t3.shop_names) shop_names,
	       DECODE(t4.brand_name, t3.brand_name, '1', '0') is_exist
		  from (select t.brand_name,
		               max(t.add_date) last_add_date,
		               count(1) counts,
		               listagg(t.shop_name, ',') shop_names
		          from (select t1.*, t2.shop_name
		                  from JXC_BRAND_APPLY t1, entp_shop t2
		                 where t1.shop_id = t2.shop_id
		                 order by t2.shop_name) t
		         group by t.brand_name) t3
		  left join BASE_BRAND_INFO t4 on t3.brand_name = t4.brand_name
		 order by t3.counts desc, t3.last_add_date desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>
	
