	<sql id="sf-ecVoteOption">
		<isNotEmpty prepend=" and " property="id">t.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="title">t.TITLE = #title:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="brief">t.BRIEF = #brief:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="content">t.CONTENT = #content:CLOB#</isNotEmpty>
		<isNotEmpty prepend=" and " property="main_pic">t.MAIN_PIC = #main_pic:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="view_counts">t.VIEW_COUNTS = #view_counts:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_value">t.ORDER_VALUE = #order_value:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">t.ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_id">t.ADD_USER_ID = #add_user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">t.IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_pub">t.IS_PUB = #is_pub:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="own_sys">t.OWN_SYS = #own_sys:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="vote_id">t.VOTE_ID = #vote_id:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.title_like">t.TITLE like '%'|| #title_like# || '%'</isNotEmpty>
		
	</sql>

	<select id="selectEcVoteOption" resultMap="ecVoteOptionResult" parameterClass="ecVoteOption" cacheModel="oneDayCache">
		select t.*,(select count(*) from EC_VOTE_RECORD where vote_option_id=t.id) RECORD_COUNT  from EC_VOTE_OPTION t where 1 = 1
		<include refid="sf-ecVoteOption" />
	</select>

	<select id="selectEcVoteOptionList" resultMap="ecVoteOptionResultForList" parameterClass="ecVoteOption" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from (
		select t.*,(select count(*) from EC_VOTE_RECORD where vote_option_id=t.id) RECORD_COUNT from EC_VOTE_OPTION t where 1 = 1
		<include refid="sf-ecVoteOption" />
		) order by <isNotEmpty prepend=" " property="map.order_by_record"> RECORD_COUNT desc,</isNotEmpty>ORDER_VALUE desc,ID asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectEcVoteOptionCount" resultClass="long" parameterClass="ecVoteOption" cacheModel="oneDayCache">
		select count(*) from EC_VOTE_OPTION t where 1 = 1
		<include refid="sf-ecVoteOption" />
	</select>

	<select id="selectEcVoteOptionPaginatedList" resultMap="ecVoteOptionResult" parameterClass="ecVoteOption" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from (
		select t.*,(select count(*) from EC_VOTE_RECORD where vote_option_id=t.id) RECORD_COUNT from EC_VOTE_OPTION t where 1 = 1
		<include refid="sf-ecVoteOption" />
		) order by <isNotEmpty prepend=" " property="map.order_by_record"> RECORD_COUNT desc,</isNotEmpty>ORDER_VALUE desc,ID asc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertEcVoteOption" parameterClass="ecVoteOption">
		<selectKey resultClass="long" keyProperty="id">select PSHOW_BASE_SEQ.nextval as id from dual </selectKey>
		<![CDATA[insert into EC_VOTE_OPTION (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="title">TITLE</isNotNull>	
			<isNotNull prepend="," property="brief">BRIEF</isNotNull>	
			<isNotNull prepend="," property="content">CONTENT</isNotNull>	
			<isNotNull prepend="," property="main_pic">MAIN_PIC</isNotNull>	
			<isNotNull prepend="," property="view_counts">VIEW_COUNTS</isNotNull>	
			<isNotNull prepend="," property="order_value">ORDER_VALUE</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
			<isNotNull prepend="," property="is_pub">IS_PUB</isNotNull>	
			<isNotNull prepend="," property="own_sys">OWN_SYS</isNotNull>	
			<isNotNull prepend="," property="vote_id">VOTE_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="title">#title:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="brief">#brief:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="content">#content:CLOB#</isNotNull>
			<isNotNull prepend="," property="main_pic">#main_pic:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="view_counts">#view_counts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">#order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="add_user_id">#add_user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_pub">#is_pub:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="own_sys">#own_sys:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="vote_id">#vote_id:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>
	
	<update id="updateEcVoteOptionViewCounts" parameterClass="EcVoteOption">
		update EC_VOTE_OPTION set VIEW_COUNTS = VIEW_COUNTS+1 where ID = #id:DECIMAL#
	</update>

	<update id="updateEcVoteOption" parameterClass="EcVoteOption">
		update EC_VOTE_OPTION
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="title">TITLE = #title:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="brief">BRIEF = #brief:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="content">CONTENT = #content:CLOB#</isNotNull>
			<isNotNull prepend="," property="main_pic">MAIN_PIC = #main_pic:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="view_counts">VIEW_COUNTS = #view_counts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID = #add_user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_pub">IS_PUB = #is_pub:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="own_sys">OWN_SYS = #own_sys:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="vote_id">VOTE_ID = #vote_id:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteEcVoteOption" parameterClass="EcVoteOption">
		delete from EC_VOTE_OPTION where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
