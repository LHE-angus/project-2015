		</isNotEmpty>
		select * from EC_BACK_PASSWORD where 1 = 1
		<include refid="sf-ecBackPassword" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectEcBackPasswordCount" resultClass="long" parameterClass="ecBackPassword" cacheModel="oneDayCache">
		select count(*) from EC_BACK_PASSWORD where 1 = 1
		<include refid="sf-ecBackPassword" />
	</select>

	<select id="selectEcBackPasswordPaginatedList" resultMap="ecBackPasswordResult" parameterClass="ecBackPassword" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from EC_BACK_PASSWORD where 1 = 1
		<include refid="sf-ecBackPassword" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertEcBackPassword" parameterClass="ecBackPassword">
