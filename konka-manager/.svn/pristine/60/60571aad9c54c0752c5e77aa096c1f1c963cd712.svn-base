	<select id="selectEcBackPassword" resultMap="ecBackPasswordResult" parameterClass="ecBackPassword" cacheModel="oneDayCache">
		select * from EC_BACK_PASSWORD where 1 = 1
		<include refid="sf-ecBackPassword" />
	</select>

	<select id="selectEcBackPasswordList" resultMap="ecBackPasswordResultForList" parameterClass="ecBackPassword" cacheModel="oneDayCache">
