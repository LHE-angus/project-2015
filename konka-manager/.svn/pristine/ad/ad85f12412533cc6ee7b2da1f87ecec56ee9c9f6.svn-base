<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="KONKA_CATEGORY">

	<typeAlias alias="konkaCategory" type="com.ebiz.mmt.domain.KonkaCategory" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertKonkaCategory" />
		<flushOnExecute statement="updateKonkaCategory" />
		<flushOnExecute statement="deleteKonkaCategory" />
	</cacheModel>

	<resultMap id="konkaCategoryResultForList" class="konkaCategory">
		<result column="C_INDEX" property="c_index" jdbcType="DECIMAL" />
		<result column="PAR_INDEX" property="par_index" jdbcType="VARCHAR" />
		<result column="C_NAME" property="c_name" jdbcType="VARCHAR" />
		<result column="C_TYPE" property="c_type" jdbcType="DECIMAL" />
		<result column="C_COMM" property="c_comm" jdbcType="VARCHAR" />
		<result column="ORDER_VALUE" property="order_value" jdbcType="DECIMAL" />
		<result column="IS_LOCK" property="is_lock" jdbcType="DECIMAL" />
		<result column="IS_DEL" property="is_del" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="konkaCategoryResult" class="konkaCategory" extends="konkaCategoryResultForList">
	</resultMap>
	
	<resultMap id="konkaCategoryResultForGroupByCCommList" class="konkaCategory">
		<result column="PAR_INDEX" property="map.par_index" jdbcType="DECIMAL" />
		<result column="C_COMM" property="map.c_comm" jdbcType="VARCHAR" />
	</resultMap>
	
	<resultMap id="konkaCategoryResultForParIndexList" class="konkaCategory">
		<result column="C_INDEX" property="map.c_index" jdbcType="DECIMAL" />
		<result column="C_NAME" property="map.c_name" jdbcType="VARCHAR" />
		<result column="PAR_INDEX" property="map.par_index" jdbcType="DECIMAL" />
		<result column="C_COMM" property="map.c_comm" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="sf-konkaCategory">
		<isNotEmpty prepend=" and " property="c_index">C_INDEX = #c_index:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="par_index">PAR_INDEX = #par_index:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_name">C_NAME = #c_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_type">C_TYPE = #c_type:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_comm">C_COMM = #c_comm:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_lock">IS_LOCK = #is_lock:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
	
		<isNotEmpty prepend=" and " property="map.c_ids">
				C_INDEX in
				<iterate close=")" open="(" conjunction="," property="map.c_ids">#map.c_ids[]#</iterate>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.c_ids2">
				C_INDEX in
				<iterate close=")" open="(" conjunction="," property="map.c_ids2">#map.c_ids2[]#</iterate>
		</isNotEmpty>
	
	</sql>

	<select id="selectKonkaCategory" resultMap="konkaCategoryResult" parameterClass="konkaCategory" cacheModel="oneDayCache">
		select * from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
	</select>

	<select id="selectKonkaCategoryList" resultMap="konkaCategoryResultForList" parameterClass="konkaCategory" cacheModel="oneDayCache">

		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
		<isEmpty prepend=" " property="map.order_by_c_com"> order by PAR_INDEX,C_INDEX asc </isEmpty>
		<isNotEmpty prepend=" " property="map.order_by_c_com"> order by C_COMM asc </isNotEmpty>
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<!-- 为适应easyUI的新方法 -->
	<select id="selectKonkaCategoryListNew" resultClass="java.util.HashMap" parameterClass="konkaCategory" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select c_index,c_name from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
		<isEmpty prepend=" " property="map.order_by_c_com"> order by PAR_INDEX,C_INDEX asc </isEmpty>
		<isNotEmpty prepend=" " property="map.order_by_c_com"> order by C_COMM asc </isNotEmpty>
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectKonkaCategoryAndParIndexList" resultMap="konkaCategoryResultForParIndexList" parameterClass="konkaCategory" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		SELECT c_index , c_name , par_index,c_comm 
  FROM KONKA_CATEGORY WHERE 1 = 1
       AND par_index IN (SELECT par_index
                           FROM KONKA_CATEGORY
                          WHERE c_type = 10
                         GROUP BY par_index)
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaCategoryGroupByCCommList" resultMap="konkaCategoryResultForGroupByCCommList" parameterClass="konkaCategory" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from ( SELECT c_comm, par_index
  			FROM KONKA_CATEGORY
 			WHERE c_type = 10
			GROUP BY c_comm,par_index)t
			order by t.par_index
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<!-- 为适应easyUI的新方法 -->
	<select id="selectKonkaCategoryGroupByCCommNew" resultClass="java.util.HashMap" parameterClass="konkaCategory" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from ( SELECT c_comm, par_index
  			FROM KONKA_CATEGORY
 			WHERE c_type = 10
			GROUP BY c_comm,par_index)t
			order by t.par_index
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaCategoryCount" resultClass="long" parameterClass="konkaCategory" cacheModel="oneDayCache">
		select count(*) from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
	</select>

	<select id="selectKonkaCategoryPaginatedList" resultMap="konkaCategoryResult" parameterClass="konkaCategory" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertKonkaCategory" parameterClass="konkaCategory">
		<selectKey resultClass="long" keyProperty="c_index">select KONKA_S_BASE.nextval as c_index from dual </selectKey>
		<![CDATA[insert into KONKA_CATEGORY (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="c_index">C_INDEX</isNotNull>	
			<isNotNull prepend="," property="par_index">PAR_INDEX</isNotNull>	
			<isNotNull prepend="," property="c_name">C_NAME</isNotNull>	
			<isNotNull prepend="," property="c_type">C_TYPE</isNotNull>	
			<isNotNull prepend="," property="c_comm">C_COMM</isNotNull>	
			<isNotNull prepend="," property="order_value">ORDER_VALUE</isNotNull>	
			<isNotNull prepend="," property="is_lock">IS_LOCK</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="c_index">#c_index:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="par_index">#par_index:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="c_name">#c_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="c_type">#c_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_comm">#c_comm:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="order_value">#order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_lock">#is_lock:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaCategory" parameterClass="KonkaCategory">
		update KONKA_CATEGORY
		<dynamic prepend="set">
			<isNotNull prepend="," property="c_index">C_INDEX = #c_index:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="par_index">PAR_INDEX = #par_index:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="c_name">C_NAME = #c_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="c_type">C_TYPE = #c_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_comm">C_COMM = #c_comm:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_lock">IS_LOCK = #is_lock:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="c_index">C_INDEX = #c_index#</isNotEmpty>
		<isEmpty prepend=" " property="c_index">
			<isNotEmpty prepend=" " property="map.pks">
				C_INDEX in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteKonkaCategory" parameterClass="KonkaCategory">
		delete from KONKA_CATEGORY where
		<isNotEmpty prepend=" " property="c_index">C_INDEX = #c_index#</isNotEmpty>
		<isEmpty prepend=" " property="c_index">
			<isNotEmpty prepend=" " property="map.pks">
				C_INDEX in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>
	
	
	<!-- R3客户合并中的合并客户类型查询 -->
	<select id="selectKonkaCategoryMapList" resultClass="java.util.HashMap" parameterClass="konkaCategory" cacheModel="oneDayCache">
		select c_index,c_name from KONKA_CATEGORY where 1 = 1
		<include refid="sf-konkaCategory" />
	</select>

</sqlMap>