	<select id="selectKonkaParagonShowinfoForView" resultMap="konkaParagonShowinfoResultForView" parameterClass="konkaParagonShowinfo" >
		select a.* , b.dept_name as part_name,d.user_name as user_name
		  from KONKA_PARAGON_SHOWINFO a
		  left join konka_dept b on b.dept_id = a.part_company_id
		                        and b.dept_type = 3
		   left join konka_pe_prod_user d on d.id = a.addman
		     and d.is_del=0
		 where 1 = 1
		<include refid="sf-konkaParagonShowinfo" />
	</select>
	
