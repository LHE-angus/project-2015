			<isNotNull prepend="," property="mp_sn">MP_SN</isNotNull>	
			<isNotNull prepend="," property="lng">LNG</isNotNull>	
			<isNotNull prepend="," property="lat">LAT</isNotNull>	
			<isNotNull prepend="," property="c_ip">C_IP</isNotNull>	
		</dynamic>
		,ADD_DATE
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="mp_sn">#mp_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="lng">#lng:VARCHAR#</isNotNull>
