			<isNotNull prepend="," property="mp_sn">MP_SN = #mp_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="lng">LNG = #lng:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="lat">LAT = #lat:VARCHAR#</isNotNull>
