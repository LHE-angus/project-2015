	<select id="selectKonkaMobileUserGpsTrackPaginatedList" resultMap="konkaMobileUserGpsTrackResult" parameterClass="konkaMobileUserGpsTrack" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from KONKA_MOBILE_USER_GPS_TRACK where 1 = 1
