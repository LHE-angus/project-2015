	</select>
	
	<select id="selectKonkaMobileUserGpsTrackAndYwyCount" resultClass="long" parameterClass="konkaMobileUserGpsTrack" cacheModel="oneDayCache">
		select count(1)
		    from (
		    		select t.mp_sn,max(t.add_date) add_date 
		    		from konka_mobile_user_gps_track t 
		    		where 1 = 1
		    		<isNotEmpty prepend=" and " property="map.add_date_between">trunc(ADD_DATE) = to_date(#map.add_date_between:VARCHAR#,'yyyy-mm-dd')</isNotEmpty>
		    		group by t.mp_sn) t1
		    left join konka_pe_prod_user t2 on t2.link_phone = t1.mp_sn
		    left join (select tt1.user_id,to_char(listagg(tt2.role_name, ',')) role_name from konka_pe_role_user tt1
						left join konka_pe_role_info tt2 on tt2.role_id = tt1.role_id group by tt1.user_id) t3 on t3.user_id = t2.id
		where 1 = 1
		<isNotEmpty prepend=" and " property="mp_sn">t1.MP_SN = #mp_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.user_mp_like">
			<![CDATA[   upper(t2.user_name) like '%' || upper(#map.user_mp_like#) || '%' 
						or t1.MP_SN like '%' || #map.user_mp_like# || '%' 
		    ]]>
	    </isNotEmpty>
	</select>

	<select id="selectKonkaMobileUserGpsTrackAndYwyPaginatedList" resultMap="konkaMobileUserGpsTrackAndYwyResult" parameterClass="konkaMobileUserGpsTrack" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select t1.*,t2.user_name,t2.department dept_name,t3.role_name
		    from (
		    		select t.mp_sn,max(t.add_date) add_date 
		    		from konka_mobile_user_gps_track t 
		    		where 1 = 1
		    		<isNotEmpty prepend=" and " property="map.add_date_between">trunc(ADD_DATE) = to_date(#map.add_date_between:VARCHAR#,'yyyy-mm-dd')</isNotEmpty>
		    		group by t.mp_sn) t1
		 	left join konka_pe_prod_user t2 on t2.link_phone = t1.mp_sn
		    left join (select tt1.user_id,to_char(listagg(tt2.role_name, ',')) role_name from konka_pe_role_user tt1
						left join konka_pe_role_info tt2 on tt2.role_id = tt1.role_id group by tt1.user_id) t3 on t3.user_id = t2.id
		where 1 = 1
		<isNotEmpty prepend=" and " property="mp_sn">t1.MP_SN = #mp_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.user_mp_like">
			<![CDATA[   upper(t2.user_name) like '%' || upper(#map.user_mp_like#) || '%' 
						or t1.MP_SN like '%' || #map.user_mp_like# || '%' 
		    ]]>
	    </isNotEmpty>
		<!-- order by ID asc -->
		order by t1.add_date desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertKonkaMobileUserGpsTrack" parameterClass="konkaMobileUserGpsTrack">
