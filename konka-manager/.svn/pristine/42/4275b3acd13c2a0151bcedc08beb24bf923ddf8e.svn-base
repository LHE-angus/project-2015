package com.ebiz.mmt.domain;

import java.io.Serializable;
import java.math.BigDecimal;

import com.ebiz.ssi.domain.BaseDomain;

/**
 * @author Xu,XiaoYuan
 * @version 2012-03-02 09:12
 */
public class KonkaXxZmdRewardSetPd extends BaseDomain implements Serializable {

	private static final long serialVersionUID = -1L;

	private Long pd_set_id;
	
	private Long dept_id;
	
	private Long zmd_id;
	
	private String md_name;
	
	private Long reward_type;
	
	private BigDecimal reward_ratio;
	
	public KonkaXxZmdRewardSetPd() {

	}

	public Long getPd_set_id() {
		return pd_set_id;
	}

	public void setPd_set_id(Long pd_set_id) {
		this.pd_set_id = pd_set_id;
	}
	
	public Long getDept_id() {
		return dept_id;
	}

	public void setDept_id(Long dept_id) {
		this.dept_id = dept_id;
	}
	
	public Long getZmd_id() {
		return zmd_id;
	}

	public void setZmd_id(Long zmd_id) {
		this.zmd_id = zmd_id;
	}
	
	public String getMd_name() {
		return md_name;
	}

	public void setMd_name(String md_name) {
		this.md_name = md_name;
	}
	
	public Long getReward_type() {
		return reward_type;
	}

	public void setReward_type(Long reward_type) {
		this.reward_type = reward_type;
	}
	
	public BigDecimal getReward_ratio() {
		return reward_ratio;
	}

	public void setReward_ratio(BigDecimal reward_ratio) {
		this.reward_ratio = reward_ratio;
	}
	
}