package com.ebiz.mmt.domain;

import java.io.Serializable;

import com.ebiz.ssi.domain.BaseDomain;

/**
 * @author Xu,ZhengYang
 * @date 2011-02-09 02:25:40
 */
public class SysModuleRoleCanGrant extends BaseDomain implements Serializable {

	private static final long serialVersionUID = -1L;
	
	private Long id;
	
	private Long role_id;
	
	private Integer mod_id;
	
	private Integer ppdm_code;
	
	public SysModuleRoleCanGrant() {

	}
	
	public void setId(Long id) {
		this.id = id;
	}
	
	public Long getId() {
		return this.id;
	}
	
	public void setRole_id(Long role_id) {
		this.role_id = role_id;
	}
	
	public Long getRole_id() {
		return this.role_id;
	}
	
	public void setMod_id(Integer mod_id) {
		this.mod_id = mod_id;
	}
	
	public Integer getMod_id() {
		return this.mod_id;
	}
	
	public void setPpdm_code(Integer ppdm_code) {
		this.ppdm_code = ppdm_code;
	}
	
	public Integer getPpdm_code() {
		return this.ppdm_code;
	}
	
}