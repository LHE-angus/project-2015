@charset "utf-8";
/* CSS Document */
/*main*/
.maincont {width:1200px;margin:0 auto;clear:both;}
/*head*/
#topbox {width:100%;margin:0 auto;clear:both;background:url(../images/topbg1.gif) repeat-x top;}
#topcont {width:1200px;margin:0 auto;clear:both;}
#topt {height:26px;border-bottom:1px solid #cfcfcf;}
.top1 {width:1200px;height:21px;margin:0 auto;clear:both;padding-top:5px;}
.topl {float:left;width:60%;height:21px;font-size:12px;font-weight:normal;line-height:21px;margin-left:28px;display:inline;}
.topl h3{float:left;padding-right:5px;color:#999;}
.topl h4 {float:left;padding-right:30px;display:inline;}
.topl cite {float:left;vertical-align:middle;margin-bottom:1px;}
.topl strong {font-weight:bold;font-family:"Microsoft YaHei","宋体";}
.topr {float:right;font-size:12px;font-weight:normal;line-height:21px;}
.topr a {padding:0 5px;}
.k_logo {float: left;margin:50px 0 39px 37px;display:inline;}
.k_top_r {float: right;margin:46px 49px 35px 0;display:inline;}
.k_topsearch {float:left;width:385px;margin:1px 56px 0 0;color:#666;}
.k_topsearch1 {float:left;width:385px;height:28px;}
.searchinput {float:left;width:294px;height:28px;}
.input_search {width:280px;height:28px;padding:0 7px;background:url(../images/inputbg.gif) no-repeat;line-height:28px;border:0;}
.searchbut {float:left;width:91px;height:28px;background:url(../images/but_search.gif) no-repeat;}
.k_tel {float:left;}

/*--------------------------------touch--------------------------------*/
/*left*/
.touch_left {float:left;width:217px;margin:10px 11px 0 0;display:inline;}
.touch_tit1 {margin:0 auto;clear:both;height:31px;background:url(../images/ico_touch.gif) #a9a8a8 no-repeat 9% 50%;padding:5px 0 0 40px;font:bold 14px/31px "宋体";color:#fff;}
.touchtab1 {float:left;width:215px;border-right:1px solid #e4e4e4;border-left:1px solid #e4e4e4;}
.touchtit2 {margin:0 auto;clear:both;height:33px;background:url(../images/arrow7.gif) #f5f5f5 no-repeat 9% 50%;border-top:1px solid #e4e4e4;border-bottom:1px solid #e4e4e4;font:bold 14px/33px "宋体";color:#000;padding-left:40px;}
.touchul1 li {margin:0 auto;clear:both;height:35px;background:url(../images/line2.gif) #fff no-repeat 20% 50%;border-bottom:1px solid #e4e4e4;font:normal 12px/35px "宋体";padding-left:58px;cursor:pointer;}

/*right*/
.touch_right {float:left;width:972px;margin-top:10px;}
.position {margin:0 auto;clear:both;height:32px;background:url(../images/ico_home2.gif) #efefef no-repeat 14px center;padding:4px 0 0 36px;line-height:32px;color:#6c6c6c;}
.position a,.position a:visited {color:#6c6c6c;}
.position a:hover {color:#FF0000;text-decoration:underline;}
.touchtab2 {margin:21px 0 18px 14px;clear:both;}
.touchtab2 h3 {font:normal 14px/30px "宋体";color:#5c5c5c;}
.touchtab2 h4 {font-family:"Microsoft YaHei","宋体";font-size:14px;line-height:30px;color:#5c5c5c;}
.touchtab2 h4 a,.touchtab2 h4 a:visited {color:#5c5c5c;}
.touchtab2 h4 a:hover {color:#FF0000;text-decoration:underline;}
.touchtab3 {margin-left:4px;}
.touch_table1 {border-collapse:collapse;border:1px solid #d7d7d7;}
.touch_table1 td {padding:18px 10px 15px 10px;border:1px solid #d7d7d7;}
.touch_tdh3 {float:left;font:normal 14px "宋体";color:#5c5c5c;}
.touch_tdh4 {float:left;font:normal 14px "宋体";color:#939393;}
.touch_tdh4 a,.touch_tdh4 a:visited {color:#939393;}
.touch_tdh4 a:hover {color:#FF0000;text-decoration:underline;}

.touchtit3 {float:left;width:100%;height:25px;background:url(../images/titlibg6.gif) bottom repeat-x;margin:20px 0 20px 4px;display:inline;}
.touchtit3 li {float:left;width:129px;height:25px;background:url(../images/titbg7.gif) no-repeat top;font-family:"Microsoft YaHei","宋体";font-size:14px;line-height:24px;text-align:center;margin-right:8px;display:inline;cursor:pointer;}
.touchtit3 .curli {float:left;width:129px;height:23px;background:#fff;margin:1px 8px 0 0;display:inline;border:1px solid #0262a7;border-bottom:0;}
.touchtab4 {float:left;width:968px;margin-left:4px;display:inline;}
.touchul2 li {float:left;width:162px;height:181px;margin:0 12px 0 15px;display:inline;}
.touchlipic {width:145px;margin:0 auto;clear:both;text-align:left;}
.touchlitit {margin:0 auto;clear:both;font:normal 12px/24px "宋体";color:#555;}
.fontblue14,a.fontblue14,.fontblue14 a {font-weight:bold;font-size:12px;color:#015a96;}
.midtxt {text-decoration:line-through;}
.touchtab4 i{ display:block;float:left;overflow:hidden;}
.to_left,.to_right{ width:23px;height:123px;float:left;}
.to_left{ padding:27px 37px 0 0;display:inline;}
.to_left a,.to_right a{ width:23px;height:123px;display:block;cursor:pointer;}
.to_right{ padding:27px 0 0 0;}
.touchulbox3 {float:left;width:885px;overflow:hidden;}
.touchul3 {float:left;width:4000px;}
.touchul3 li {float:left;width:110px;margin-right:38px;display:inline;overflow:hidden;}
.padbot45 {padding-bottom:45px;}
/*foot*/
.bottom1 {float:left;width:1198px;border:1px solid #e3e3e3;border-bottom:0;margin-top:10px;}
.bottomul {margin:20px 0;clear:both;}
.bottomul li {float:left;width:13%;height:130px;margin:0 39px;display:inline;}
.bottomul li h3 {border-bottom:1px solid #e3e3e3;background:url(../images/arrow2.gif) no-repeat;padding:0 0 5px 20px;font:bold 12px "宋体";color:#000;margin-bottom:4px;}
.bottomul li p {display:block;margin-left:10px;background:url(../images/dot.gif) no-repeat 0 10px;padding-left:15px;line-height:24px;}
.bottomul .nomar {margin-right:0;!important;}

.bottom2 {float:left;width:1198px;height:87px;border:1px solid #e3e3e3;background:#f1f1f1;}
.b_service_pic_sub {float:left;margin:26px 58px 0 58px;display:inline;}
.bottom2 span {float:right;margin:26px 72px 0 0;display:inline;}
.foot {margin:0 auto;clear:both;padding:30px 0;text-align:center;font-size:14px;}
.foot span {font-family:Arial, Helvetica, sans-serif;}

/*form*/
.inputbtn {float:left;width:94px;height:28px;}
.input_txt {width:301px;height:28px;border:1px solid #ccc;padding:0 2px;font:normal 12px/32px "宋体";color:#999;}

.touch_form_table0 {margin-top:12px;border-collapse:collapse;}
.touch_form_table0 td {padding:8px 10px 6px 10px;}
.touch_form_table1 {margin-top:12px;border-collapse:collapse;border:1px solid #d7d7d7;}
.touch_form_table1 td {padding:8px 10px 6px 10px;border:1px solid #d7d7d7;}
.touch_form_table2 {margin-top:12px;border-collapse:collapse;}
.touch_form_table2 .tr1{height:32px;background:#e9e9e9;}
.touch_form_table2 td {padding:8px 10px 6px 10px;border-bottom:1px solid #d7d7d7;}

.bti{background:url(../images/use.gif) 0 -42px;width:50px;height:25px;margin-top:-1px}
.bti{border:none}
.btn{display:inline-block;*zoom:1;padding-right:13px;}
.btn s{float:left;width:3px;margin-right:10px;}
.btn,.btn s{background:url(../images/btn.png) no-repeat;}
.btn-4{line-height:21px;}
.btn-4 s{height:21px;}
.btn-4{background-position:right -209px;}
.btn-4 s{background-position:0 -187px;}
a.btn-4:link,a.btn-4:visited{color:#ffffff;}
a.btn-again {background: url(../images/use.gif);display: block;height: 21px;line-height: 100px; margin: 3px auto 0;overflow: hidden;width: 72px;}
a.btn-again:hover{background-position:0 -21px}


