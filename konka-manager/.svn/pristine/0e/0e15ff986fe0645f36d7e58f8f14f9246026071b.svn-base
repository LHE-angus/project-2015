package com.ebiz.mmt.domain;

import java.io.Serializable;

import com.ebiz.ssi.domain.BaseDomain;

public class SxExtendedPermissions extends BaseDomain implements Serializable {

	private static final long serialVersionUID = -1L;

	/**
	 * ID
	 */
	private java.lang.Long id;

	/**
	 * 用户ID
	 */
	private java.lang.Long user_id;

	/**
	 * 供应商、品牌、地区表中的ID
	 */
	private String expand_id;

	/**
	 * 1:供应商 2:品牌 3:地区 4:品类 5:品牌_品类
	 */
	private java.lang.Integer expand_type;

	public SxExtendedPermissions() {
	}

	public void setId(java.lang.Long id) {
		this.id = id;
	}

	public java.lang.Long getId() {
		return this.id;
	}

	public void setUser_id(java.lang.Long user_id) {
		this.user_id = user_id;
	}

	public java.lang.Long getUser_id() {
		return this.user_id;
	}

	public void setExpand_id(java.lang.String expand_id) {
		this.expand_id = expand_id;
	}

	public java.lang.String getExpand_id() {
		return this.expand_id;
	}

	public void setExpand_type(java.lang.Integer expand_type) {
		this.expand_type = expand_type;
	}

	public java.lang.Integer getExpand_type() {
		return this.expand_type;
	}

}