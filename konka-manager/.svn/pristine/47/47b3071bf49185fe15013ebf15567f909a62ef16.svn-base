<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="JF_RULE">

	<typeAlias alias="jfRule" type="com.ebiz.mmt.domain.JfRule" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertJfRule" />
		<flushOnExecute statement="updateJfRule" />
		<flushOnExecute statement="deleteJfRule" />
	</cacheModel>

	<resultMap id="jfRuleResultForList" class="jfRule">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="JF_NAME" property="jf_name" jdbcType="VARCHAR" />
		<result column="DEPT_ID" property="dept_id" jdbcType="DECIMAL" />
		<result column="PD_ID" property="pd_id" jdbcType="VARCHAR" />
		<result column="JF_TYPE" property="jf_type" jdbcType="DECIMAL" />
		<result column="JF_VALUE" property="jf_value" jdbcType="DECIMAL" />
		<result column="JF_AVL_TYPE" property="jf_avl_type" jdbcType="DECIMAL" />
		<result column="JF_AVL_START" property="jf_avl_start" jdbcType="DATETIME" />
		<result column="JF_AVL_END" property="jf_avl_end" jdbcType="DATETIME" />
		<result column="JF_STYLE" property="jf_style" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="DATETIME" />
	</resultMap>

	<resultMap id="jfRuleResult" class="jfRule" extends="jfRuleResultForList">
	</resultMap>

	<sql id="sf-jfRule">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_name">JF_NAME = #jf_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_id">PD_ID = #pd_id:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_type">JF_TYPE = #jf_type:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_value">JF_VALUE = #jf_value:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_avl_type">JF_AVL_TYPE = #jf_avl_type:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_avl_start">JF_AVL_START = #jf_avl_start:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_avl_end">JF_AVL_END = #jf_avl_end:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="jf_style">JF_STYLE = #jf_style:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">ADD_DATE = #add_date:DATETIME#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.jf_avl_start_lt"><![CDATA[ JF_AVL_START < to_date(#map.jf_avl_start_lt# || '23:59:59','yyyy-mm-dd hh24:mi:ss') ]]></isNotEmpty>
		<isNotEmpty prepend=" and " property="map.jf_avl_end_gt"><![CDATA[ JF_AVL_END > to_date(#map.jf_avl_end_gt# || '00:00:00','yyyy-mm-dd hh24:mi:ss') ]]></isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.pd_id_like">upper(PD_ID) like '%' || upper(#map.pd_id_like:VARCHAR#) || '%'</isNotEmpty>
	</sql>

	<select id="selectJfRule" resultMap="jfRuleResult" parameterClass="jfRule" cacheModel="oneDayCache">
		select * from JF_RULE where 1 = 1
		<include refid="sf-jfRule" />
	</select>

	<select id="selectJfRuleList" resultMap="jfRuleResultForList" parameterClass="jfRule" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from JF_RULE where 1 = 1
		<include refid="sf-jfRule" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectJfRuleCount" resultClass="long" parameterClass="jfRule" cacheModel="oneDayCache">
		select count(*) from JF_RULE where 1 = 1
		<include refid="sf-jfRule" />
	</select>

	<select id="selectJfRulePaginatedList" resultMap="jfRuleResult" parameterClass="jfRule" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from JF_RULE where 1 = 1
		<include refid="sf-jfRule" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertJfRule" parameterClass="jfRule">
		<selectKey resultClass="long" keyProperty="id">select seq_jf_rule.nextval as id from dual </selectKey>
		<![CDATA[insert into JF_RULE (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="jf_name">JF_NAME</isNotNull>	
			<isNotNull prepend="," property="dept_id">DEPT_ID</isNotNull>	
			<isNotNull prepend="," property="pd_id">PD_ID</isNotNull>	
			<isNotNull prepend="," property="jf_type">JF_TYPE</isNotNull>	
			<isNotNull prepend="," property="jf_value">JF_VALUE</isNotNull>	
			<isNotNull prepend="," property="jf_avl_type">JF_AVL_TYPE</isNotNull>	
			<isNotNull prepend="," property="jf_avl_start">JF_AVL_START</isNotNull>	
			<isNotNull prepend="," property="jf_avl_end">JF_AVL_END</isNotNull>	
			<isNotNull prepend="," property="jf_style">JF_STYLE</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_name">#jf_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="dept_id">#dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="pd_id">#pd_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="jf_type">#jf_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_value">#jf_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_avl_type">#jf_avl_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_avl_start">#jf_avl_start:DATETIME#</isNotNull>
			<isNotNull prepend="," property="jf_avl_end">#jf_avl_end:DATETIME#</isNotNull>
			<isNotNull prepend="," property="jf_style">#jf_style:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:DATETIME#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateJfRule" parameterClass="JfRule">
		update JF_RULE
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_name">JF_NAME = #jf_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="pd_id">PD_ID = #pd_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="jf_type">JF_TYPE = #jf_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_value">JF_VALUE = #jf_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_avl_type">JF_AVL_TYPE = #jf_avl_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="jf_avl_start">JF_AVL_START = #jf_avl_start:DATETIME#</isNotNull>
			<isNotNull prepend="," property="jf_avl_end">JF_AVL_END = #jf_avl_end:DATETIME#</isNotNull>
			<isNotNull prepend="," property="jf_style">JF_STYLE = #jf_style:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:DATETIME#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteJfRule" parameterClass="JfRule">
		delete from JF_RULE where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>