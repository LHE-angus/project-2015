<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="JXC_PD_TYPE">

	<typeAlias alias="jxcPdType" type="com.ebiz.mmt.domain.JxcPdType" />

	

	<resultMap id="jxcPdTypeResultForList" class="jxcPdType">
