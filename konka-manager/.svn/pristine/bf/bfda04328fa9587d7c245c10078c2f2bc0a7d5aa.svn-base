		<isNotEmpty prepend=" and " property="add_user_id">ADD_USER_ID = #add_user_id:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectJxcPdType" resultMap="jxcPdTypeResult" parameterClass="jxcPdType" >
		select * from CHEA_FILL.JXC_PD_TYPE where 1 = 1
		<include refid="sf-jxcPdType" />
	</select>

	<select id="selectJxcPdTypeList" resultMap="jxcPdTypeResultForList" parameterClass="jxcPdType" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from CHEA_FILL.JXC_PD_TYPE where 1 = 1
		<include refid="sf-jxcPdType" />
		order by ADD_DATE desc, ID desc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectJxcPdTypeCount" resultClass="long" parameterClass="jxcPdType" >
		select count(*) from CHEA_FILL.JXC_PD_TYPE where 1 = 1
		<include refid="sf-jxcPdType" />
	</select>

	<select id="selectJxcPdTypePaginatedList" resultMap="jxcPdTypeResult" parameterClass="jxcPdType" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from CHEA_FILL.JXC_PD_TYPE where 1 = 1
		<include refid="sf-jxcPdType" />
		 order by ADD_DATE desc, ID desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertJxcPdType" parameterClass="jxcPdType">
		<selectKey resultClass="long" keyProperty="id">select CHEA_FILL.JXC_BASE_SEQ.nextval as id from dual </selectKey>
		<![CDATA[insert into CHEA_FILL.JXC_PD_TYPE (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="shop_id">SHOP_ID</isNotNull>	
			<isNotNull prepend="," property="name">NAME</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="shop_id">#shop_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="name">#name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="add_date">sysdate</isNotNull>
			<isNotNull prepend="," property="add_user_id">#add_user_id:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateJxcPdType" parameterClass="JxcPdType">
		update CHEA_FILL.JXC_PD_TYPE
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="shop_id">SHOP_ID = #shop_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="name">NAME = #name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:DATE#</isNotNull>
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID = #add_user_id:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteJxcPdType" parameterClass="JxcPdType">
		delete from CHEA_FILL.JXC_PD_TYPE where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>