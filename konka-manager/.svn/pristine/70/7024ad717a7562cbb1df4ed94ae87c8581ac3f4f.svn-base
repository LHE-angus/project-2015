	<select id="selectKonkaPeArticleTypePaginatedList" resultMap="konkaPeArticleTypeWithNameResult" parameterClass="konkaPeArticleType" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select a.*,  b.user_name , c.dept_name
		from KONKA_PE_Article_Type a 
		left join KONKA_PE_PROD_USER b on a.add_user_id = b.id 
		left join KONKA_DEPT c on a.dept_id=c.dept_id
		where 1 = 1
		<include refid="sf-konkaPeArticleType" />
		order by a.add_datetime desc, a.id desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

