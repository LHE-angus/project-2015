	<select id="selectKonkaPeArticleType" resultMap="konkaPeArticleTypeWithNameResult" parameterClass="konkaPeArticleType" >
		select a.*,b.user_name,s.dept_name from konka_pe_article_type a 
		left join konka_pe_prod_user b on a.add_user_id=b.id
		left join KONKA_DEPT s on a.dept_id=s.dept_id  where 1 = 1
		<include refid="sf-konkaPeArticleType" />
	</select>

	<select id="selectKonkaPeArticleTypeList" resultMap="konkaPeArticleTypeResultForList" parameterClass="konkaPeArticleType" >
