<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="KONKA_JOBS">

	<typeAlias alias="konkaJobs" type="com.ebiz.mmt.domain.KonkaJobs" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertKonkaJobs" />
		<flushOnExecute statement="updateKonkaJobs" />
		<flushOnExecute statement="deleteKonkaJobs" />
	</cacheModel>

	<resultMap id="konkaJobsResultForList" class="konkaJobs">
		<result column="FUN_NAME" property="fun_name" jdbcType="VARCHAR" />
		<result column="JOB_DESC" property="job_desc" jdbcType="VARCHAR" />
		<result column="STATUS" property="status" jdbcType="INTEGER" />
		<result column="EXCUT_TIME" property="excut_time" jdbcType="VARCHAR" />
		<result column="UPDATE_TIME" property="update_time" jdbcType="TIMESTAMP" />
		<result column="UPDATED_NAME" property="updated_name" jdbcType="VARCHAR" />
		<result column="UPDATED_ID" property="updated_id" jdbcType="BIGINT" />
	</resultMap>

	<resultMap id="konkaJobsResult" class="konkaJobs" extends="konkaJobsResultForList">
	</resultMap>

	<sql id="sf-konkaJobs">
		<isNotEmpty prepend=" and " property="fun_name">FUN_NAME = #fun_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="job_desc">JOB_DESC = #job_desc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="status">STATUS = #status:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="excut_time">EXCUT_TIME = #excut_time:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_time">UPDATE_TIME = #update_time:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="updated_name">UPDATED_NAME = #updated_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="updated_id">UPDATED_ID = #updated_id:BIGINT#</isNotEmpty>
	</sql>

	<select id="selectKonkaJobs" resultMap="konkaJobsResult" parameterClass="konkaJobs" cacheModel="oneDayCache">
		select * from KONKA_JOBS where 1 = 1
		<include refid="sf-konkaJobs" />
	</select>

	<select id="selectKonkaJobsList" resultMap="konkaJobsResultForList" parameterClass="konkaJobs" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_JOBS where 1 = 1
		<include refid="sf-konkaJobs" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaJobsCount" resultClass="long" parameterClass="konkaJobs" cacheModel="oneDayCache">
		select count(*) from KONKA_JOBS where 1 = 1
		<include refid="sf-konkaJobs" />
	</select>

	<select id="selectKonkaJobsPaginatedList" resultMap="konkaJobsResult" parameterClass="konkaJobs" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from KONKA_JOBS where 1 = 1
		<include refid="sf-konkaJobs" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertKonkaJobs" parameterClass="konkaJobs">
		<![CDATA[insert into KONKA_JOBS (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="fun_name">FUN_NAME</isNotNull>	
			<isNotNull prepend="," property="job_desc">JOB_DESC</isNotNull>	
			<isNotNull prepend="," property="status">STATUS</isNotNull>	
			<isNotNull prepend="," property="excut_time">EXCUT_TIME</isNotNull>	
			<isNotNull prepend="," property="update_time">UPDATE_TIME</isNotNull>	
			<isNotNull prepend="," property="updated_name">UPDATED_NAME</isNotNull>	
			<isNotNull prepend="," property="updated_id">UPDATED_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="fun_name">#fun_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="job_desc">#job_desc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="status">#status:INTEGER#</isNotNull>
			<isNotNull prepend="," property="excut_time">#excut_time:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="update_time">#update_time:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="updated_name">#updated_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="updated_id">#updated_id:BIGINT#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaJobs" parameterClass="KonkaJobs">
		update KONKA_JOBS
		<dynamic prepend="set">
			<isNotNull prepend="," property="fun_name">FUN_NAME = #fun_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="job_desc">JOB_DESC = #job_desc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="status">STATUS = #status:INTEGER#</isNotNull>
			<isNotNull prepend="," property="excut_time">EXCUT_TIME = #excut_time:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="update_time">UPDATE_TIME = #update_time:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="updated_name">UPDATED_NAME = #updated_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="updated_id">UPDATED_ID = #updated_id:BIGINT#</isNotNull>
		</dynamic>
		where FUN_NAME=#fun_name:VARCHAR#
	</update>

	<delete id="deleteKonkaJobs" parameterClass="KonkaJobs">
		delete from KONKA_JOBS where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>