		<isNotEmpty prepend=" and " property="update_uid">t1.UPDATE_UID = #update_uid:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">t1.IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.ywy_name_like">	         
	        t1.USER_ID in (select ID from KONKA_PE_PROD_USER where REAL_NAME like '%' || #map.ywy_name_like:VARCHAR# || '%')
		</isNotEmpty>
