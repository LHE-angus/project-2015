		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.konka_dept_id">t1.dept_id in (select dept_id from  konka_dept t start with  t.dept_id=#map.konka_dept_id#  connect by prior t.dept_id= t.par_id )</isNotEmpty>
