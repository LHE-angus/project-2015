	<sql id="sf-konkaUserMobile">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_id">USER_ID = #user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_name">USER_NAME = #user_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="real_name">REAL_NAME = #real_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="mobile_no">MOBILE_NO = #mobile_no:VARCHAR#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="ywy_crc">YWY_CRC = #ywy_crc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_activate">IS_ACTIVATE = #is_activate:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="entp_crc">ENTP_CRC = #entp_crc:VARCHAR#</isNotEmpty>		
		<isNotEmpty prepend=" and " property="setplan_crc">SETPLAN_CRC = #setplan_crc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="confirm_date">CONFIRM_DATE = #confirm_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="use_status">USE_STATUS = #use_status:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="create_date">CREATE_DATE = #create_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="create_uid">CREATE_UID = #create_uid:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_date">UPDATE_DATE = #update_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_uid">UPDATE_UID = #update_uid:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.ywy_name_like">	         
	        USER_ID in (select ID from KONKA_PE_PROD_USER where REAL_NAME like '%' || #map.ywy_name_like:VARCHAR# || '%')
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.ywy_phone">
	         MOBILE_NO = #map.ywy_phone:VARCHAR#
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.konka_dept_id">dept_id in (select dept_id from  konka_dept t start with  t.dept_id=#map.konka_dept_id#  connect by prior t.dept_id= t.par_id )</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.ywy_user_id">user_id = #map.ywy_user_id#</isNotEmpty>

		<isNotEmpty prepend=" and " property="map.dept_id">(dept_id in (select dept_id from  konka_dept t start with  t.dept_id=#map.dept_id#  connect by prior t.dept_id= t.par_id )
		<isNotEmpty prepend=" or " property="map.user_id">user_id = #map.user_id#)</isNotEmpty>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.update_time_st"><![CDATA[mobile_no in (select mobile_no from konka_user_gps_info b where b.update_time>=to_date(#map.update_time_st#,'yyyy-MM-dd'))]]></isNotEmpty>
		<isNotEmpty prepend=" and " property="map.update_time_en"><![CDATA[mobile_no in (select mobile_no from konka_user_gps_info b where b.update_time<=to_date(#map.update_time_en#,'yyyy-MM-dd'))]]></isNotEmpty>
	</sql>
	
	<sql id="sf-konkaUserMobile-t1">
		<isNotEmpty prepend=" and " property="id">t1.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_id">t1.USER_ID = #user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_name">t1.USER_NAME = #user_name:VARCHAR#</isNotEmpty>
