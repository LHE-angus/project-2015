	<select id="selectKonkaUserMobile" resultMap="konkaUserMobileResult" parameterClass="konkaUserMobile" >
		select * from KONKA_USER_MOBILE where 1 = 1
		<include refid="sf-konkaUserMobile" />
	</select>

	<select id="selectKonkaUserMobileList" resultMap="konkaUserMobileResultForList" parameterClass="konkaUserMobile" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_USER_MOBILE a where 1 = 1
		<include refid="sf-konkaUserMobile" />
		order by ID asc, MOBILE_NO asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>	
	</select>
	
	<select id="selectKonkaUserMobileAndGpsList" resultMap="konkaUserMobileAndGpsResultForList" parameterClass="konkaUserMobile" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select t1.*,t2.gps_type,t2.blongitude,t2.blatitude,to_char(t2.update_time,'yyyy-mm-dd hh24:mi:ss') update_time,t2.address,t3.full_name,t4.duty from Konka_User_Mobile t1
		left join konka_dept_tree_view t3 on t3.dept_id = t1.dept_id
		left join (select mobile_no,gps_type,blongitude,blatitude,update_time,address from 
					(select mobile_no,gps_type,blongitude,blatitude,update_time,address,row_number() over(partition by mobile_no order by id desc) rn 
						from Konka_User_Gps_Info 
					 where blongitude is not null and blatitude is not null
					 <isNotEmpty prepend=" and " property="map.update_time_st"><![CDATA[update_time>=to_date(#map.update_time_st#,'yyyy-MM-dd')]]></isNotEmpty>
					 <isNotEmpty prepend=" and " property="map.update_time_en"><![CDATA[update_time<=to_date(#map.update_time_en#,'yyyy-MM-dd' )]]></isNotEmpty>
					 ) t
					where t.rn = 1
				  ) t2 on t2.mobile_no = t1.user_name
		left join (select tt1.user_id,to_char(listagg(tt2.role_name, ',')) duty from KONKA_PE_ROLE_USER tt1
		            left join KONKA_PE_ROLE_INFO tt2 on tt2.role_id = tt1.role_id 
		            group by tt1.user_id) t4 on t4.user_id = t1.user_id
		where t2.blongitude is not null and t2.blatitude is not null
		<include refid="sf-konkaUserMobile-t1" />
		order by t1.ID asc, t1.MOBILE_NO asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>	
	</select>

	<select id="selectKonkaUserMobileCount" resultClass="long" parameterClass="konkaUserMobile" >
		select count(*) from KONKA_USER_MOBILE where 1 = 1
		<include refid="sf-konkaUserMobile" />
	</select>

	<select id="selectKonkaUserMobilePaginatedList" resultMap="konkaUserMobileResult" parameterClass="konkaUserMobile" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
			select * from KONKA_USER_MOBILE where 1=1
		<include refid="sf-konkaUserMobile" />
		    order by ID asc, MOBILE_NO asc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>	
	</select>

