	<sql id="sf-jBaseStore-ForR3">
		<isNotEmpty prepend=" and " property="store_id">a.STORE_ID = #store_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="store_sn">a.STORE_SN = #store_sn:VARCHAR#</isNotEmpty>
