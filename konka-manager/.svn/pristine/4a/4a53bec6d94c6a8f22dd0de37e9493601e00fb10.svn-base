		<isNotEmpty prepend=" and " property="is_dft_buy_store">a.IS_DFT_BUY_STORE = #is_dft_buy_store:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_dft_sell_store">a.IS_DFT_SELL_STORE = #is_dft_sell_store:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_id">a.C_ID = #c_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.store_sn_like">a.STORE_SN like '%' || #map.store_sn_like:VARCHAR# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.store_name_like">a.STORE_NAME like '%' || #map.store_name_like:VARCHAR# || '%'</isNotEmpty>
	</sql>
    
   

