	<select id="selectJBaseStore" resultMap="jBaseStoreResult" parameterClass="jBaseStore" cacheModel="oneDayCache">
		select * from J_BASE_STORE where 1 = 1
		<include refid="sf-jBaseStore" />
	</select>

	<select id="selectJBaseStoreList" resultMap="jBaseStoreResultForList" parameterClass="jBaseStore" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from J_BASE_STORE where 1 = 1
		<include refid="sf-jBaseStore" />
		<isNotEmpty prepend=" " property="map.order_by_is_dft_buy_store_desc">order by IS_DFT_BUY_STORE desc</isNotEmpty>
		<isNotEmpty prepend=" " property="map.order_by_is_dft_sell_store_desc">order by IS_DFT_SELL_STORE desc</isNotEmpty>
		<isNotEmpty prepend=" " property="map.add_date">ORDER BY ADD_DATE desc</isNotEmpty>
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectJBaseStoreCount" resultClass="long" parameterClass="jBaseStore" cacheModel="oneDayCache">
		select count(*) from J_BASE_STORE where 1 = 1
		<include refid="sf-jBaseStore" />
	</select>

	<select id="selectJBaseStorePaginatedList" resultMap="jBaseStoreResult" parameterClass="jBaseStore" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from J_BASE_STORE where 1 = 1
		<include refid="sf-jBaseStore" />
		order by store_id desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>
	
	<!-- Xiao,GuoJian -->
	<select id="selectJBaseStoreForR3PaginatedList" resultMap="jBaseStoreResultForR3" parameterClass="jBaseStore" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select a.*,b.sale_r3_code,b.r3_code from J_BASE_STORE a left join J_BASE_STORE_R3 b on a.store_id = b.store_id where 1 = 1 
		<include refid="sf-jBaseStore-ForR3" />
		order by a.store_id desc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>
	
	<!-- Xiao,GuoJian -->
	<select id="selectJBaseStoreForR3List" resultMap="jBaseStoreResultForR3" parameterClass="jBaseStore" cacheModel="oneDayCache">
		select a.*,b.sale_r3_code,b.r3_code from J_BASE_STORE a left join J_BASE_STORE_R3 b on a.store_id = b.store_id where 1 = 1 
		<include refid="sf-jBaseStore-ForR3" />
		order by a.store_id desc
	</select>
	
	<select id="selectstoresnweforlist" resultMap="jBaseStoreR3Result" parameterClass="jBaseStoreR3" cacheModel="oneDayCache">
		<![CDATA[ select * from (SELECT distinct(Sale_r3_code) FROM ( ]]>
   select column_5 sale_r3_code from  channel_data_import  where 1=1
   <isNotEmpty prepend="and" property="r3_code">
  column_1= #r3_code#
  </isNotEmpty>
     GROUP BY column_5
       union 
      select SONGDF_ID sale_r3_code from KONKA_PARTERSHIP  where 1=1
      <isNotEmpty prepend="and" property="r3_code">
      SHOUDF_ID= #r3_code#
      </isNotEmpty>
       GROUP BY SONGDF_ID
        <isNotEmpty prepend="  " property="r3_code">
       union 
       select #r3_code# from dual
       </isNotEmpty>
  <![CDATA[ ) T WHERE  Sale_r3_code NOT IN ( select  distinct(sale_r3_code) STORE_SN_WE from j_base_store_R3 sr3 where R3_CODE = #r3_code# AND sr3.SALE_R3_CODE is not null)  )]]>
	</select>
	
	<select id="selectjbasestoreanddetails" resultMap="jBaseStoreanddetailsResult" parameterClass="jBaseStore" cacheModel="oneDayCache">
		select a.*,b.id,b.r3_code,b.memo,b.is_del,b.add_date,b.sale_r3_code,b.sale_r3_name
		 from J_BASE_STORE a left join J_BASE_STORE_r3 b on a.store_id=b.store_id  where 1 = 1
		<include refid="sf-jBaseStore-for-details" />
	</select>
	
	

	<insert id="insertJBaseStore" parameterClass="jBaseStore">
		<selectKey resultClass="long" keyProperty="store_id">select SEQ_J_STORE_ID.nextval as store_id from dual </selectKey>
		<![CDATA[insert into J_BASE_STORE (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="store_id">STORE_ID</isNotNull>	
			<isNotNull prepend="," property="store_sn">STORE_SN</isNotNull>	
			<isNotNull prepend="," property="store_name">STORE_NAME</isNotNull>	
			<isNotNull prepend="," property="store_addr">STORE_ADDR</isNotNull>	
			<isNotNull prepend="," property="remarks">REMARKS</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="is_dft_buy_store">IS_DFT_BUY_STORE</isNotNull>	
			<isNotNull prepend="," property="is_dft_sell_store">IS_DFT_SELL_STORE</isNotNull>	
			<isNotNull prepend="," property="c_id">C_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="store_id">#store_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="store_sn">#store_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="store_name">#store_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="store_addr">#store_addr:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="remarks">#remarks:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="is_dft_buy_store">#is_dft_buy_store:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_dft_sell_store">#is_dft_sell_store:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_id">#c_id:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateJBaseStore" parameterClass="JBaseStore">
		update J_BASE_STORE
		<dynamic prepend="set">
			<isNotNull prepend="," property="store_id">STORE_ID = #store_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="store_sn">STORE_SN = #store_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="store_name">STORE_NAME = #store_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="store_addr">STORE_ADDR = #store_addr:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="remarks">REMARKS = #remarks:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="is_dft_buy_store">IS_DFT_BUY_STORE = #is_dft_buy_store:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_dft_sell_store">IS_DFT_SELL_STORE = #is_dft_sell_store:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_id">C_ID = #c_id:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="store_id">STORE_ID = #store_id#</isNotEmpty>
		<isNotEmpty prepend=" " property="c_id">C_ID = #c_id#</isNotEmpty>
		<isEmpty prepend=" " property="store_id">
			<isNotEmpty prepend=" " property="map.pks">
				STORE_ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteJBaseStore" parameterClass="JBaseStore">
		update J_BASE_STORE set IS_DEL = 1 where
		<isNotEmpty prepend=" " property="store_id">STORE_ID = #store_id#</isNotEmpty>
		<isEmpty prepend=" " property="store_id">
			<isNotEmpty prepend=" " property="map.pks">
				STORE_ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

	<!-- 返回Map -->
	<select id="selectJBaseStoreMapList" resultClass="java.util.HashMap" parameterClass="jBaseStore" cacheModel="oneDayCache">
