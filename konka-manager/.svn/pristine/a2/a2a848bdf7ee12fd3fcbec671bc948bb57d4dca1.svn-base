	 */
	public String getPd_name() {
		return pd_name;
	}

	/**
	 * @val 产品型号
	 */
	public void setPd_name(String pdName) {
		pd_name = pdName;
	}

	/**
	 * @val 数量
	 */
	public Integer getGood_count() {
		return good_count;
	}

	/**
	 * @val 数量
	 */
	public void setGood_count(Integer goodCount) {
		good_count = goodCount;
	}

	/**
	 * @val 单位
	 */
	public String getGood_unit() {
		return good_unit;
	}

	/**
	 * @val 单位
	 */
	public void setGood_unit(String goodUnit) {
		good_unit = goodUnit;
	}

	/**
	 * @val 单价
	 */
	public BigDecimal getGood_price() {
		return good_price;
	}

	/**
	 * @val 单价
	 */
