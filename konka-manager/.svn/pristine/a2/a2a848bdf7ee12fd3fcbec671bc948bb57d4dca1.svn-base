package com.ebiz.mmt.web.struts.inter.form;

import java.math.BigDecimal;


/**
 * @author Xiao,GuoJian
 * @since 2014-09-24
 * @see 订单详情页面   产品明细
 */
public class KonkaOrderInfoDetailsSearch extends BaseInterForm {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1L;
	
	private String pd_name;//产品型号
	
	private Integer good_count;//数量
	
