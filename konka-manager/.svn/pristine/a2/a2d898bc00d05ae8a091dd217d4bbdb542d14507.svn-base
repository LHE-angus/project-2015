@charset "utf-8";
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video,a,input,select{margin:0;padding:0;}
html{font-size:312.5%;}
body{background-color:#fff;color:#000;font-size:0.28rem;line-height:1;font-family:"微软雅黑";}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block;}
h1,h2,h3,h4,h5,h6{font-weight:inherit;}
div{vertical-align:top;}
ul{list-style:none;}
blockquote,q{quotes:none;}
blockquote:before,blockquote:after,q:before,q:after{content:none;}
a{text-decoration:none;color:inherit;}
img{border:0;vertical-align:top;max-width:100%;outline:none;}
ins{background-color:#ff9;color:#000;text-decoration:none;}
mark{background-color:#ff9;color:#000;font-style:italic;font-weight:bold;}
del{text-decoration:line-through;}
abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help;}
table{border:0;border-collapse:collapse;border-spacing:0;}
hr{display:block;height:1px;border:0;border-top:1px solid #ccc;margin:0;padding:0;}
input,select{border:0;vertical-align:top;outline:none;}
.tc{text-align:center;}
.tr{text-align:right;}
.red{color:#cb0000;}
.red2{color:#ed5565;}
.blue{color:#0267b7;}
.gray{color:#383838;}
.ft30{font-size:0.5rem;}
.sizing_border{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;}
.layout_box{display:-webkit-box;display:-moz-box;display:-o-box;display:-ms-flexbox;display:box;}
.box_v_c{-webkit-box-align:center;-moz-box-align:center;-o-box-align:center;-ms-flex-align:center;box-align:center;}
/*** header ***/
.navbar{width:100%;height:1.4rem;}
.navbar-left .backpic{display:inline-block;padding:0 0.36rem;}
.navbar-center{-webkit-box-flex:1;-moz-box-flex:1;-o-box-flex:1;-ms-flex:1;box-flex:1;font-size:0.6rem;text-align:center;}
.navbar-c-search{position:relative;display:inline-block;padding:0 0.36rem;width:100%;}
.navbar-center .searchpic{position:absolute;left:0.72rem;top:0.28rem;}
.navbar-c-search input{padding-left:1.1rem;width:100%;height:1rem;border:1px solid #999;border-radius:3px;font-size:0.48rem;font-family:"微软雅黑";}
.navbar-right .icopic{display:inline-block;padding:0 0.36rem;}
.navbar-r-list{display:inline-block;padding:0.24rem 0.36rem 0;text-align:center;}
.navbar-r-list p{margin-top:0.08rem;line-height:0.56rem;font-size:0.36rem;}

/*** map ***/
.main-map{border-top:3px solid #e62e2e;}
.mapshow{width:100%;text-align:center;}
.map-contentlist{padding:0 0.48rem;border-top:1px solid #cecccd;}
.map-list-info{padding:0.13rem 0 0.3rem;width:100%;}
.map-list-info-l{-webkit-box-flex:1;-moz-box-flex:1;-o-box-flex:1;-ms-flex:1;box-flex:1;}
.map-list-info-l h3{line-height:0.96rem;font-size:0.48rem;}
.map-list-info-l p{line-height:0.6rem;font-size:0.36rem;}
.map-list-info-r{padding-right:0.36rem;}
.map-list-info-r a{color:#e62e2e;font-size:0.48rem;vertical-align:middle;}
.map-list-info-r img{vertical-align:middle;}
/*** 列表 ***/
.main-listmap{background-color:#f3f4f8;border-top:3px solid #e62e2e;}
.listmap-list{padding-top:0.06rem;}
.listmap-list li{margin-top:0.34rem;padding:0.38rem 0.28rem 0.28rem;width:100%;background-color:#fff;border-top:1px solid #d2d2d2;border-bottom:1px solid #d2d2d2;}
.listmap-pic{width:128px;}
.listmap-info{-webkit-box-flex:1;-moz-box-flex:1;-o-box-flex:1;-ms-flex:1;box-flex:1;padding-left:0.78rem;}
.listmap-info h3{margin-bottom:0.24rem;line-height:0.56rem;font-size:0.48rem;}
.listmap-info h3 .blue{padding-left:0.56rem;font-size:0.32rem;}
.listmap-info p {line-height:0.48rem;color:#666;font-size:0.32rem;}
/*** 网点展示 ***/
.main-wdzs{padding-top:0.2rem;background-color:#f3f4f8;border-top:1px solid #dedee0;}
.wdzs-des{padding:0.36rem 0.28rem 0.28rem;width:100%;background-color:#fff;}
.wdzs-des-info{-webkit-box-flex:1;-moz-box-flex:1;-o-box-flex:1;-ms-flex:1;box-flex:1;padding-left:0.76rem;}
.wdzs-des-info h3{line-height:1.04rem;font-size:0.48rem;}
.wdzs-des-info h3 span{padding-left:0.56rem;font-size:0.32rem;}
.wdzs-des-info p{line-height:0.48rem;color:#666;font-size:0.32rem;}
.wdzs-des-pf{padding-left:1.5rem;}
.wdzs-detail{margin-top:0.34rem;padding-top:0.4rem;background-color:#fff;}
.cont-nav-list{border-bottom:2px solid #e40000;}
.cont-nav-list li{display:inline-block;width:25%;line-height:0.96rem;color:#383838;font-size:0.48rem;text-align:center;}
.cont-nav-list li.current{color:#e40000;border-bottom:2px solid #e40000;}
.wdzs-cont{line-height:0.6rem;font-size:0.36rem;}
.wdzs-contA{padding:0.54rem 0.6rem 2rem;}
.wdzs-contA img{display:block;margin:0 auto;}
.wdzs-contA p{margin-top:0.76rem;color:#666;text-indent:2em;}
/*** 数据分析 ***/
.main-sjfx{background-color:#f0f2f5;border-top:1px solid #e62e2e;}
.cont-nav-list2{border-top:1px solid #c72f2f;}
.cont-nav-list2 li{display:inline-block;width:30%;line-height:1.16rem;font-size:0.3rem;border-bottom:3px solid #e3e4e8;border-right:1px solid #edeef2;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;text-align:center;}
.cont-nav-list2 li.current{background-color:#ed5565;color:#fff;border-bottom-color:#da4453;border-right-color:#ec7683;}
.cont-nav-list2 li:last-child{width:5%;border-right:none;}
.sjfx-contA,.sjfx-contB,.sjfx-contC,.sjfx-contD,.sjfx-contE{padding:0 0.24rem 1rem;}
.sjfx-cont-t{height:1.6rem;line-height:1.6rem;color:#e62e2e;font-size:0.35rem;}
.sjfx-cont-list{padding-top:0.06rem;}
.sjfx-cont-list li{margin-top:0.04rem;width:100%;background-color:#fff;line-height:1.2rem;color:#323232;font-size:0.3rem;}
.sjfx-cont-list li:nth-of-type(odd){background-color:#f9fafc;}
.sjfx-cont-list li span{display:inline-block;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box;border-right:1px solid #f0f2f5;}
.sjfx-cont-list li span:nth-child(1){width:30%;padding-left:0.48rem;} 
.sjfx-cont-list li span:nth-child(2),.sjfx-cont-list li span:nth-child(3){width:30%;text-align:center;}
.sjfx-cont-list li span:nth-child(3){border-right:none;}
.sjfx-cont-list li span:nth-child(3) img{vertical-align:text-top;}
/*** 产品详情 ***/
.main-cpxq{background-color:#f0f2f5;border-top:1px solid #e62e2e;}
.cpshow{background-color:#fff;border-top:1px solid #c62f2f;}
.cpshow-pic{padding:2rem 0.2rem 0;border-top:2px solid #f0f2f5;}
.cpshow-pic div{display:block;margin:0 auto;text-align:center;}
.cpshow-pic p{padding:0.5rem 0.2rem 0.4rem;height:1.28rem;text-align:right;border-bottom:2px solid #f7f7f7;}
.cpshow-pic p span{display:inline-block;width:1.28rem;height:1.28rem;background-color:#b1b1b1;line-height:1.28rem;color:#fff;font-size:0.4rem;text-align:center;border-radius:50%;}
.cpshow-info{padding:0.64rem 0.48rem 0.58rem;}
.cpshow-info h3{line-height:0.72rem;font-size:0.56rem;}
.cpshow-info .layout_box{margin-top:0.4rem;width:100%;}
.cpshow-info .price{-webkit-box-flex:1;-moz-box-flex:1;-o-box-flex:1;-ms-flex:1;box-flex:1;font-size:0.6rem;}
.cpshow-info .pfxl{padding-right:0.4rem;display:inline-block;}
.cpshow-info span{display:block;line-height:0.72rem;color:#545454;font-size:0.36rem;}
.cpshow-info .price span:nth-child(1){color:#e50000;font-size:0.6rem;}
.cpshow-info .price span:nth-child(2){text-decoration:line-through;}
.cpshow-info .pfxl span{text-align:center;}
.cpshow-info .pfxl img{margin-left:5px;vertical-align:text-top;}
.cpxq-des{margin-top:0.32rem;padding:0.32rem 0;background-color:#fff;}
.cpxq-cont{padding-top:0.1rem;}
.cpxq-cont-list li{margin-top:1rem;text-align:center;}
.buy-btn{position:fixed;bottom:0;width:100%;height:1.4rem;background-color:#d4d4d4;}
.buy-btn span{display:inline-block;width:50%;}
.buy-btn span a{margin:0 auto;display:block;width:80%;height:1.12rem;line-height:1.12rem;color:#fff;font-size:0.48rem;text-align:center;border-radius:6px;}
.onlinebuy a{background-color:#fd6305;}
.offlinebuy a{background-color:#cb0000;}
.buy-btn span a img{margin-top:-5px;margin-right:0.5rem;vertical-align:middle;}
/*** 切换 ***/
.qhContList>:first-child~*{display:none;}