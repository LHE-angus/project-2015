		if ("0".equals(pay_way)) {
			xml += "<AddedService  name='" + name + "' value='" + value + "' value1='" + value1 + "' value2='" + value2
			        + "' value3='" + value3 + "' value4='" + value4 + "'>";
			xml += "</AddedService>";
		}
		if ("0".equals(s_price)) {
			xml += "<AddedService  name='" + price_name + "' value='" + price + "'>";
			xml += "</AddedService>";
		}
		xml += "</OrderOption>";
