		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.init_date_s">
			<![CDATA[a.INIT_DATE >= to_date(#map.init_date_s#,'yyyy-MM-dd hh24:mi:ss')]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.init_date_e">
			<![CDATA[a.INIT_DATE <= to_date(#map.init_date_e#,'yyyy-MM-dd hh24:mi:ss')]]>
		</isNotEmpty>
	</sql>

	<select id="selectJBaseGoodsInitStock" resultMap="jBaseGoodsInitStockResult" parameterClass="jBaseGoodsInitStock" cacheModel="oneDayCache">
		select a.*, b.GOODS_NAME, c.STORE_NAME from J_BASE_GOODS_INIT_STOCK a 
       LEFT JOIN J_BASE_GOODS b
          ON b.GOODS_ID = a.GOODS_ID
       LEFT JOIN J_BASE_STORE c
          ON c.STORE_ID = a.STORE_ID
           where 1 = 1
		<include refid="sf-jBaseGoodsInitStock" />
	</select>

