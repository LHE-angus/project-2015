@charset "utf-8";
/* CSS Document */
.top{height:417px; background:url(../images/top.jpg) top center no-repeat; overflow:hidden;}
.nav{width:970px; height:48px; margin:234px auto 0px;}
.nav ul li{float:left; height:31px; padding-top:17px;}
.nav ul li.line{height:34px; padding:14px 11px 0px 11px;}
.lookresult{width:970px; margin:5px auto 0px; text-align:right;}
.main{background:#0d2d5e url(../images/mainbg.jpg) top center no-repeat; overflow:hidden;}
.votebox{width:920px; margin:0px auto; overflow:hidden;}
.vote{width:150px; height:268px; padding:0px 11px; float:left; margin:0px 12px 35px 0px; background:url(../images/votebg.jpg) no-repeat; text-align:center;}
.vote_tit{height:41px; line-height:41px; font-size:18px; color:#fff; font-family:"微软雅黑";}
.vote_num{height:28px; line-height:24px; color:#336598; font-size:14px;}
.votebox .mr{margin:0px 0px 35px 0px;}
.voteresult_title{width:944px; margin:49px auto;}
.voteresult{width:944px; border-bottom:1px #06070c solid; margin:0px auto; overflow:hidden;}
.voteresult_list{width:943px; height:110px; border-top:1px #06070c solid; border-right:1px #06070c solid;}
.voteresult_list_l{width:87px; border-right:1px #06070c solid; float:left;}
.voteresult_list_r{width:830px; height:78px; padding:16px 0px 16px 25px; float:left; background:url(../images/voteresult_list_rbg.jpg) no-repeat;}
.voteresult_pic{width:78px; margin:0px 22px 0px 0px; float:left;}
.voteresult_con{width:535px; height:43px; float:left;}
.voteresult_tit{height:33px; font-size:18px; color:#fff; font-family:"微软雅黑";}
.voteresult_cd{width:535px; height:9px; border-top:1px #06070c solid; background:#272c3f;}
.voteresult_cd .width80{width:446px; height:9px; background:#72b5c6;}
.voteresult_num{width:120px; height:78px; line-height:78px; font-size:32px; color:#96ccce; font-family:Arial; float:right;}
.description{width:918px; height:1380px; padding:30px; margin:50px auto 40px; }
.description_tit{height:40px; font-size:18px; color:#ffffc9; font-family:"微软雅黑";}
.description_cont{font-size:14px; color:#fff; line-height:22px;}
.votephase_l{width:100px; float:left; margin-right:18px;}
.votephase_r{width:800px; float:left;}
.votephase_r h1{font-size:26px; height:40px; color:#fff; font-family:"微软雅黑"; font-weight:normal;}
.votephase_r h1 span{color:#e1bb66; padding-left:23px;}
.votephase_r p{font-size:14px; color:#fff; line-height:22px;}
.foot{height:383px; background:url(../images/foot.jpg) top center no-repeat;}
.top2{height:400px;background:#0d2d5e url(../images/top2.jpg) top center no-repeat; overflow:hidden;} 
