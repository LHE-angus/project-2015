@charset "gb2312";

/*--------------------index--------------------*/
/*top*/
.topcont {width:100%;margin:0 auto;clear:both;height:63px;background:url(../images/kjtopbg1.gif) repeat-x;border-bottom:1px #a80404 solid;}
.topleft {width:347px;height:47px;padding:16px 0 0 13px;}
.rtopcont {background:url(../images/kjtopbg2.gif) no-repeat top right;padding:4px 77px 0 0;font:normal 12px "宋体";color:#fff;text-align:right;}
.rtoptab {width:59px;height:49px;text-align:center;padding-top:6px;cursor:pointer;}
.rtoptab h3 {padding-bottom:6px;}
.rtoptab a,.rtoptab a:visited {color:#fff;}
.rtoptab a:hover {color:#fff000;text-decoration:none;}
.rtoptab_cur {width:59px;height:49px;border:1px #f1987b solid; text-align:center;padding-top:6px;cursor:pointer;color:#fff;}
.rtoptab_cur h3 {padding-bottom:6px;}
.rtoptab_cur a,.rtoptab_cur a:visited {color:#fff;}
.rtoptab_cur a:hover {color:#fff000;text-decoration:none;}


/*left*/
.leftFramesTable {
	border-top: 1px #cecece solid;
	border-left: 1px #cecece solid;
	text-align: center;
}
.leftFramesTable th{
	border-right: 1px #cecece solid;
	border-bottom: 1px #cecece solid;
	height:22px;background:url(../images/leftit1.gif) no-repeat;
	font:bold 12px "宋体";color:#cc0000; 
	padding:8px 0 0 18px;
}
.leftFramesTable td{
	border-right: 1px #cecece solid;
	border-bottom: 1px #cecece solid;
	height:22px;background:url(../images/leftitbg1.gif) repeat-x;font:normal 12px "宋体";color:#333;
    padding:8px 0 0 0;
}
.leftFramesTable .oalnav_cur{height:22px;background:#fff;font:normal 12px "宋体";color:#cc0000;padding:8px 0 0 0;}
.leftFramesTable .oalnav_cur a,.leftFramesTable .oalnav_cur a:visited {color:#cc0000;}
.leftFramesTable .oalnav_cur a:hover {color:#ff0064;text-decoration:none;}

/*mid*/
.oararrow {float:left;width:6px;height:100%;padding-top:213px;display:inline;}


/*right*/
.oartop {color:#74685f; margin-bottom: 10px;margin-top:10px;margin-left:7px; height: 32px; line-height: 32px; margin: 0 auto; background:url(admin-index/images/positionbg.jpg) repeat-x #efe7e7;  border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: #C9CFCD;}
/*.oartop {margin:0 auto;clear:both;height:18px;background:url(../images/manager/rtitbg1.gif) repeat-x;padding:5px 0 0 17px;line-height:18px;color:#333;border-bottom:1px #cfcdcd solid;}*/
.oartop a,.oartop a:visited {color:#333;}
.oartop a:hover {color:#ff0064;text-decoration:underline;}
.rtabcont1 {margin:8px 7px;clear:both;}
.tableTop {border:1px #ccc solid;}
.tableTop td {padding:10px 0 10px 10px;}
.rtable2 {border-top:1px #ccc solid;border-left:1px #ccc solid;}
.rtable2 td {border-right:1px #e3e3e3 solid;border-bottom:1px #e3e3e3 solid;padding:5px 18px 2px 18px;}
.rtable2 .tabtt1 {height:23px;background:url(../images/tabtitbg1.gif) repeat-x;}
.rtable2 .tabtt1 td {border-right:1px #e3e3e3 solid;border-bottom:1px #ccc solid;padding:7px 18px 0 18px;font:bold 12px "宋体";color:#666;}
.rtabcont2 {margin:8px 7px;clear:both;padding:10px 30px;border:1px #ccc solid;}
.rtable3 {margin:0 auto;clear:both;}
.rtable3 td {padding:7px 0 2px 0;}
.rtabcont3 {margin:8px 7px 87px 7px;clear:both;}
.rtabcont4 {margin:8px 7px 260px 7px;clear:both;}


/*bottom*/
.oabottom {width:100%;height:20px;margin:0 auto;clear:both;background:url(../images/bottom.gif) repeat-x;}

.tableClass {
	width: 100%;
	background-color:#ccc;
}
.tableClass th {
	background-image:url(../images/tabtitbg1.gif);
	height: 23px;
	line-height: 23px;
	text-align: center;
	font-size:12px; 
	color:#333; 
	font-weight:bold;
}
.tableClass tr {
}
.tableClass tr.title_top td {
	background-color: #f5f4f4;
	height: 23px;
	line-height: 23px;
	text-align: center;
	font-size:12px; 
	font-weight:bold;
}
.tableClass tr.title_top_no_bc td {
	height: 23px;
	line-height: 23px;
	text-align: center;
	font-size:12px; 
	font-weight:bold;
}
.tableClass td {
	line-height: 28px;
	background-color: #ffffff;
}
.tableClass td.title_item {
	background-color:#f5f4f4;
	font-weight:bold;
	text-align:right;
}
.tableClass td.title_item_no_bc {
	font-weight:bold;
	text-align:right;
}

