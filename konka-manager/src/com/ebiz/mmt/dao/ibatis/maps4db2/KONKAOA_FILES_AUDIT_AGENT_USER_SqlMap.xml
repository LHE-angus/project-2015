	<select id="selectKonkaoaAgentFilesAuditPopedomCount" resultClass="long" parameterClass="filesAuditAgentUser" >
		select count(*)
		  from (select * from KONKAOA_files_property t where t.link_id = #map.link_id#) a
		  left join (select * from KONKAOA_files_audit_popedom t
		              where t.link_id = (select id
		                                   from KONKAOA_FILES_AUDIT_AGENT_USER
		                                  where user_id = #user_id# and is_del = 0
		                                    and agent_user_id = #agent_user_id#)) b on a.c_index = b.c_index
		 where b.id is null
	</select>

	<select id="selectKonkaoaFilesAuditAgentUserCount" resultClass="long" parameterClass="filesAuditAgentUser" >
		select count(*) from KONKAOA_FILES_AUDIT_AGENT_USER where 1 = 1
