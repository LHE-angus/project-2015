<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="FROM_DRP_ORDER_DETAIL">
	<typeAlias alias="fromDrpOrderDetail" type="com.ebiz.mmt.domain.FromDrpOrderDetail" />
	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertFromDrpOrderDetail" />
		<flushOnExecute statement="updateFromDrpOrderDetail" />
		<flushOnExecute statement="deleteFromDrpOrderDetail" />
	</cacheModel>

	<resultMap id="fromDrpOrderDetailResultForList" class="fromDrpOrderDetail">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<!--  -->
		<result column="DS_ORDER_FK_ID" property="ds_order_fk_id" jdbcType="VARCHAR" />
		<result column="PD_NAME" property="pd_name" jdbcType="VARCHAR" />
		<result column="GOOD_COUNT" property="good_count" jdbcType="BIGINT" />
		<result column="GOOD_PRICE" property="good_price" jdbcType="DECIMAL" />
		<result column="GOOD_SUM_PRICE" property="good_sum_price" jdbcType="DECIMAL" />
		<result column="GOOD_DISCOUNT" property="good_discount" jdbcType="DECIMAL" />
		<result column="GOOD_DISCOUNT_PRICE" property="good_discount_price" jdbcType="DECIMAL" />
		<result column="PD_REMARK" property="pd_remark" jdbcType="VARCHAR" />
		<!-- ds_order_id 明细的原始主键 -->
		<result column="DS_ORDER_ID" property="ds_order_id" jdbcType="VARCHAR" />
	</resultMap>

	<resultMap id="fromDrpOrderDetailResult" class="fromDrpOrderDetail" extends="fromDrpOrderDetailResultForList">
	</resultMap>

	<sql id="sf-fromDrpOrderDetail">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="ds_order_fk_id">DS_ORDER_FK_ID = #ds_order_fk_id:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_name">PD_NAME = #pd_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="good_count">GOOD_COUNT = #good_count:BIGINT#</isNotEmpty>
		<isNotEmpty prepend=" and " property="good_price">GOOD_PRICE = #good_price:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="good_sum_price">GOOD_SUM_PRICE = #good_sum_price:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="good_discount">GOOD_DISCOUNT = #good_discount:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="good_discount_price">GOOD_DISCOUNT_PRICE = #good_discount_price:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_remark">PD_REMARK = #pd_remark:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="ds_order_id">DS_ORDER_ID = #ds_order_id:VARCHAR#</isNotEmpty>
	</sql>

	<select id="selectFromDrpOrderDetail" resultMap="fromDrpOrderDetailResult" parameterClass="fromDrpOrderDetail" cacheModel="oneDayCache">
		select * from FROM_DRP_ORDER_DETAIL where 1 = 1
		<include refid="sf-fromDrpOrderDetail" />
	</select>

	<select id="selectFromDrpOrderDetailList" resultMap="fromDrpOrderDetailResultForList" parameterClass="fromDrpOrderDetail" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from FROM_DRP_ORDER_DETAIL where 1 = 1
		<include refid="sf-fromDrpOrderDetail" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectFDrpOrderDetailAndQddataList" resultMap="fromDrpOrderDetailResultForList" parameterClass="fromDrpOrderDetail" cacheModel="oneDayCache">
		<![CDATA[ 
			SELECT b.DS_ORDER_ID,
			       b.DS_ORDER_FK_ID,
			       b.ID,
			       d.PD_NAME,
			       d.GOOD_COUNT,
			       d.GOOD_PRICE,
			       d.GOOD_DISCOUNT,
			       d.GOOD_DISCOUNT_PRICE,
			       d.GOOD_SUM_PRICE,
			       d.PD_REMARK
			  FROM FROM_DRP_ORDER a
			       LEFT JOIN FROM_DRP_ORDER_DETAIL b
			          ON a.DS_ORDER_ID = b.DS_ORDER_FK_ID
			       LEFT JOIN KONKA_ORDER_INFO c
			          ON a.QD_TRAN_INDEX = c.TRADE_INDEX
			       LEFT JOIN KONKA_ORDER_INFO_DETAILS d
			          ON c.ID = d.ORDER_ID
			 WHERE 1 = 1 AND b.PD_NAME = d.PD_NAME
		 ]]>
		 <isNotEmpty prepend=" and " property="ds_order_id">a.DS_ORDER_ID = #ds_order_id:VARCHAR#</isNotEmpty>
	</select>

	<select id="selectFromDrpOrderDetailCount" resultClass="long" parameterClass="fromDrpOrderDetail" cacheModel="oneDayCache">
		select count(*) from FROM_DRP_ORDER_DETAIL where 1 = 1
		<include refid="sf-fromDrpOrderDetail" />
	</select>

	<select id="selectFromDrpOrderDetailPaginatedList" resultMap="fromDrpOrderDetailResult" parameterClass="fromDrpOrderDetail" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from FROM_DRP_ORDER_DETAIL where 1 = 1
		<include refid="sf-fromDrpOrderDetail" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>
	

	<insert id="insertFromDrpOrderDetail" parameterClass="fromDrpOrderDetail">
		<selectKey resultClass="long" keyProperty="id">select FROM_DRP_ORDER_DETAIL_S.nextval as id from dual </selectKey>
		<![CDATA[insert into FROM_DRP_ORDER_DETAIL (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="ds_order_fk_id">DS_ORDER_FK_ID</isNotNull>	
			<isNotNull prepend="," property="pd_name">PD_NAME</isNotNull>	
			<isNotNull prepend="," property="good_count">GOOD_COUNT</isNotNull>	
			<isNotNull prepend="," property="good_price">GOOD_PRICE</isNotNull>	
			<isNotNull prepend="," property="good_sum_price">GOOD_SUM_PRICE</isNotNull>	
			<isNotNull prepend="," property="good_discount">GOOD_DISCOUNT</isNotNull>	
			<isNotNull prepend="," property="good_discount_price">GOOD_DISCOUNT_PRICE</isNotNull>	
			<isNotNull prepend="," property="pd_remark">PD_REMARK</isNotNull>	
			<isNotNull prepend="," property="ds_order_id">DS_ORDER_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="ds_order_fk_id">#ds_order_fk_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_name">#pd_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="good_count">#good_count:BIGINT#</isNotNull>
			<isNotNull prepend="," property="good_price">#good_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_sum_price">#good_sum_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_discount">#good_discount:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_discount_price">#good_discount_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="pd_remark">#pd_remark:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="ds_order_id">#ds_order_id:VARCHAR#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateFromDrpOrderDetail" parameterClass="FromDrpOrderDetail">
		update FROM_DRP_ORDER_DETAIL
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="ds_order_fk_id">DS_ORDER_FK_ID = #ds_order_fk_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="pd_name">PD_NAME = #pd_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="good_count">GOOD_COUNT = #good_count:BIGINT#</isNotNull>
			<isNotNull prepend="," property="good_price">GOOD_PRICE = #good_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_sum_price">GOOD_SUM_PRICE = #good_sum_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_discount">GOOD_DISCOUNT = #good_discount:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="good_discount_price">GOOD_DISCOUNT_PRICE = #good_discount_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="pd_remark">PD_REMARK = #pd_remark:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="ds_order_id">DS_ORDER_ID = #ds_order_id:VARCHAR#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
	</update>

	<delete id="deleteFromDrpOrderDetail" parameterClass="FromDrpOrderDetail">
		delete from FROM_DRP_ORDER_DETAIL where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>