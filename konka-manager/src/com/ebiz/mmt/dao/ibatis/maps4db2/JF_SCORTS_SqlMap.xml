<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="JF_SCORTS">

	<typeAlias alias="jfScorts" type="com.ebiz.mmt.domain.JfScorts" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertJfScorts" />
		<flushOnExecute statement="updateJfScorts" />
		<flushOnExecute statement="deleteJfScorts" />
	</cacheModel>

	<resultMap id="jfScortsResultForList" class="jfScorts">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="USER_SN" property="user_sn" jdbcType="VARCHAR" />
		<result column="TOTAL_SCORTS" property="total_scorts" jdbcType="DECIMAL" />
		<result column="IS_DEL" property="is_del" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="jfScortsResult" class="jfScorts" extends="jfScortsResultForList">
	</resultMap>

	<sql id="sf-jfScorts">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_sn">USER_SN = #user_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="total_scorts">TOTAL_SCORTS = #total_scorts:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.user_sn_like">upper(USER_SN) like '%' || upper(#map.user_sn_like:VARCHAR#) || '%'</isNotEmpty>
	</sql>
	
	<sql id="sf-jfScorts-a">
		<isNotEmpty prepend=" and " property="id">a.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_sn">a.USER_SN = #user_sn:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="total_scorts">a.TOTAL_SCORTS = #total_scorts:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">a.IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.user_sn_like">upper(a.USER_SN) like '%' || upper(#map.user_sn_like:VARCHAR#) || '%'</isNotEmpty>
	</sql>

	<select id="selectJfScorts" resultMap="jfScortsResult" parameterClass="jfScorts" cacheModel="oneDayCache">
		select * from JF_SCORTS where 1 = 1
		<include refid="sf-jfScorts" />
	</select>

	<select id="selectJfScortsList" resultMap="jfScortsResultForList" parameterClass="jfScorts" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from JF_SCORTS where 1 = 1
		<include refid="sf-jfScorts" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectJfScortsForScortsList" resultMap="jfScortsResultForList" parameterClass="jfScorts" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select a.* from JF_SCORTS a left join member_info b on a.user_sn = b.user_sn  where 1 = 1
		<include refid="sf-jfScorts-a" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	

	<select id="selectJfScortsCount" resultClass="long" parameterClass="jfScorts" cacheModel="oneDayCache">
		select count(*) from JF_SCORTS where 1 = 1
		<include refid="sf-jfScorts" />
	</select>

	<select id="selectJfScortsPaginatedList" resultMap="jfScortsResult" parameterClass="jfScorts" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from JF_SCORTS where 1 = 1
		<include refid="sf-jfScorts" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertJfScorts" parameterClass="jfScorts">
		<selectKey resultClass="long" keyProperty="id">select seq_jf_base.nextval as id from dual </selectKey>
		<![CDATA[insert into JF_SCORTS (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="user_sn">USER_SN</isNotNull>	
			<isNotNull prepend="," property="total_scorts">TOTAL_SCORTS</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_sn">#user_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="total_scorts">#total_scorts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateJfScorts" parameterClass="JfScorts">
		update JF_SCORTS
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_sn">USER_SN = #user_sn:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="total_scorts">TOTAL_SCORTS = #total_scorts:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteJfScorts" parameterClass="JfScorts">
		delete from JF_SCORTS where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>