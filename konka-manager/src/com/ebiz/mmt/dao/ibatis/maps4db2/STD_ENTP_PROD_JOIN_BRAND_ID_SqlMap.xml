<sqlMap namespace="STD_ENTP_PROD_JOIN_BRAND_ID">

	<typeAlias alias="stdEntpProdJoinBrandId" type="com.ebiz.mmt.domain.StdEntpProdJoinBrandId" />

	

	<resultMap id="stdEntpProdJoinBrandIdResultForList" class="stdEntpProdJoinBrandId">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="ENTP_ID" property="entp_id" jdbcType="DECIMAL" />
		<result column="BRAND_ID" property="brand_id" jdbcType="DECIMAL" />
		<result column="OWN_SYS" property="own_sys" jdbcType="DECIMAL" />
	</resultMap>
	
	<resultMap id="stdEntpProdJoinBrandIdResult" class="stdEntpProdJoinBrandId" extends="stdEntpProdJoinBrandIdResultForList">
	</resultMap>

	<resultMap id="stdEntpProdJoinBrandIdWithEntpNameResult" class="stdEntpProdJoinBrandId" extends="stdEntpProdJoinBrandIdResultForList">
		<result column="ENTP_NAME" property="map.entp_name" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="sf-stdEntpProdJoinBrandId">
		<isNotEmpty prepend=" and " property="id">t.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="entp_id">t.ENTP_ID = #entp_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="brand_id">t.BRAND_ID = #brand_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="own_sys">t.OWN_SYS = #own_sys:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectStdEntpProdJoinBrandId" resultMap="stdEntpProdJoinBrandIdResult" parameterClass="stdEntpProdJoinBrandId" >
		select 	
			t.ID,
			t.ENTP_ID,
			t.BRAND_ID,
			t.OWN_SYS
	 from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID t where 1 = 1
		<include refid="sf-stdEntpProdJoinBrandId" />
	</select>
	
	<select id="selectStdEntpProdJoinBrandIdWithEntpName" resultMap="stdEntpProdJoinBrandIdWithEntpNameResult" parameterClass="stdEntpProdJoinBrandId" >
		select 	
			t.ID,
			t.ENTP_ID,
			t.BRAND_ID,
			t.OWN_SYS,
			SEP.ENTP_NAME
	 from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID t 
	 left join KONKA_STD_ENTP_PROD sep on sep.entp_id = t.entp_id
	 where 1 = 1
		<include refid="sf-stdEntpProdJoinBrandId" />
	</select>

	<select id="selectStdEntpProdJoinBrandIdList" resultMap="stdEntpProdJoinBrandIdResultForList" parameterClass="stdEntpProdJoinBrandId" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select 		
			t.ID,
			t.ENTP_ID,
			t.BRAND_ID,
			t.OWN_SYS
		from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID t where 1 = 1
		<include refid="sf-stdEntpProdJoinBrandId" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectStdEntpProdJoinBrandIdCount" resultClass="long" parameterClass="stdEntpProdJoinBrandId" >
		select count('1') from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID t where 1 = 1
		<include refid="sf-stdEntpProdJoinBrandId" />
	</select>

	<select id="selectStdEntpProdJoinBrandIdPaginatedList" resultMap="stdEntpProdJoinBrandIdResult" parameterClass="stdEntpProdJoinBrandId" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select 
			t.ID,
			t.ENTP_ID,
			t.BRAND_ID,
			t.OWN_SYS
		 from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID t where 1 = 1
		<include refid="sf-stdEntpProdJoinBrandId" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertStdEntpProdJoinBrandId" parameterClass="stdEntpProdJoinBrandId">
		<selectKey resultClass="long" keyProperty="id">select brand_sequence.nextval as id from dual</selectKey>
		<![CDATA[insert into KONKA_STD_ENTP_PROD_JOIN_BRAND_ID (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="entp_id">ENTP_ID</isNotNull>	
			<isNotNull prepend="," property="brand_id">BRAND_ID</isNotNull>	
			<isNotNull prepend="," property="own_sys">OWN_SYS</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="entp_id">#entp_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="brand_id">#brand_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="own_sys">#own_sys:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateStdEntpProdJoinBrandId" parameterClass="stdEntpProdJoinBrandId">
		update KONKA_STD_ENTP_PROD_JOIN_BRAND_ID
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="entp_id">ENTP_ID = #entp_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="brand_id">BRAND_ID = #brand_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="own_sys">OWN_SYS = #own_sys:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteStdEntpProdJoinBrandId" parameterClass="stdEntpProdJoinBrandId">
		delete from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

	<delete id="deleteStdEntpProdJoinBrandIdByEntpIdOrBrandId" parameterClass="stdEntpProdJoinBrandId">
		delete from KONKA_STD_ENTP_PROD_JOIN_BRAND_ID where
		<isEmpty prepend=" " property="entp_id">
			<isNotEmpty prepend=" " property="brand_id">
				BRAND_ID = #brand_id:DECIMAL#
			</isNotEmpty>
		</isEmpty>
		<isEmpty prepend=" " property="brand_id">
			<isNotEmpty prepend=" " property="entp_id">
				<isNotEmpty prepend=" " property="own_sys">
					ENTP_ID = #entp_id:DECIMAL# and OWN_SYS = #own_sys:DECIMAL#
				</isNotEmpty>
			</isNotEmpty>
		</isEmpty>
	</delete>
</sqlMap>