<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="KONKA_MOBILE_IMP_HIS">

	<typeAlias alias="konkaMobileImpHis" type="com.ebiz.mmt.domain.KonkaMobileImpHis" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertKonkaMobileImpHis" />
		<flushOnExecute statement="updateKonkaMobileImpHis" />
		<flushOnExecute statement="deleteKonkaMobileImpHis" />
	</cacheModel>

	<resultMap id="konkaMobileImpHisResultForList" class="konkaMobileImpHis">
		<result column="OPR_HIS_ID" property="opr_his_id" jdbcType="DECIMAL" />
		<result column="OPR_TITLE" property="opr_title" jdbcType="VARCHAR" />
		<result column="OPR_USER_ID" property="opr_user_id" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="DATETIME" />
		<result column="OPR_NOTE" property="opr_note" jdbcType="VARCHAR" />
		<result column="DEPT_ID" property="dept_id" jdbcType="DECIMAL" />
		<result column="DEPT_NAME" property="dept_name" jdbcType="VARCHAR" />
		<result column="OPR_OBJ" property="opr_obj" jdbcType="DECIMAL" />
		<result column="IS_DEL" property="is_del" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="konkaMobileImpHisResult" class="konkaMobileImpHis" extends="konkaMobileImpHisResultForList">
	</resultMap>
	
	<resultMap id="konkaMobileImpHisWithUserNameResult" class="konkaMobileImpHis" extends="konkaMobileImpHisResultForList">
		<result column="USER_NAME" property="map.user_name" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="sf-konkaMobileImpHis">
		<isNotEmpty prepend=" and " property="opr_his_id">OPR_HIS_ID = #opr_his_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_title">OPR_TITLE = #opr_title:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_user_id">OPR_USER_ID = #opr_user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">ADD_DATE = #add_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_note">OPR_NOTE = #opr_note:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_name">DEPT_NAME = #dept_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_obj">OPR_OBJ = #opr_obj:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectKonkaMobileImpHis" resultMap="konkaMobileImpHisResult" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		select * from KONKA_MOBILE_IMP_HIS where 1 = 1
		<include refid="sf-konkaMobileImpHis" />
	</select>

	<select id="selectKonkaMobileImpHisList" resultMap="konkaMobileImpHisResultForList" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_MOBILE_IMP_HIS where 1 = 1
		<include refid="sf-konkaMobileImpHis" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectKonkaMobileImpHisWithUserNameList" resultMap="konkaMobileImpHisWithUserNameResult" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select t.*,t2.user_name as user_name from KONKA_MOBILE_IMP_HIS t
			left join konka_pe_prod_user t2 on t.OPR_USER_ID = t2.id 
		 where 1 = 1
		<isNotEmpty prepend=" and " property="opr_obj">t.OPR_OBJ = #opr_obj:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_id">t.DEPT_ID = #dept_id:DECIMAL#</isNotEmpty>
		order by t.add_date desc
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaMobileImpHisCount" resultClass="long" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		select count(*) from KONKA_MOBILE_IMP_HIS where 1 = 1
		<include refid="sf-konkaMobileImpHis" />
	</select>

	<select id="selectKonkaMobileImpHisPaginatedList" resultMap="konkaMobileImpHisResult" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from KONKA_MOBILE_IMP_HIS where 1 = 1
		<include refid="sf-konkaMobileImpHis" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<!--Hu,Hao 部门人员数据初始化-->
	<procedure id="updateKonkaMobileImpDAtaPro" parameterClass="konkaMobileImpHis" cacheModel="oneDayCache">
		<isNotEmpty property="map.is_type_1">${call KONKA_XX_MOBILE_GEN_DEPT(#map.dept_sn:VARCHAR#,#opr_his_id:DECIMAL#)}</isNotEmpty>
		<isNotEmpty property="map.is_type_2">${call KONKA_XX_MOBILE_GEN_ROLE(#map.dept_sn:VARCHAR#,#opr_his_id:DECIMAL#)}</isNotEmpty>
		<isNotEmpty property="map.is_type_3">${call KONKA_XX_MOBILE_GEN_STORE(#map.dept_sn:VARCHAR#,#opr_his_id:DECIMAL#)}</isNotEmpty>
		<isNotEmpty property="map.is_type_4">${call KONKA_XX_MOBILE_GEN_USER(#map.dept_sn:VARCHAR#,#opr_his_id:DECIMAL#)}</isNotEmpty>
		<isNotEmpty property="map.is_type_5">${call KONKA_XX_MOBILE_GEN_ZHXY(#map.dept_sn:VARCHAR#,#opr_his_id:DECIMAL#)}</isNotEmpty>
	</procedure>
	

	<insert id="insertKonkaMobileImpHis" parameterClass="konkaMobileImpHis">
		<selectKey resultClass="long" keyProperty="opr_his_id">select OPR_HIS_ID_SEQ.nextval as opr_his_id from dual </selectKey>
		<![CDATA[insert into KONKA_MOBILE_IMP_HIS (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="opr_his_id">OPR_HIS_ID</isNotNull>	
			<isNotNull prepend="," property="opr_title">OPR_TITLE</isNotNull>	
			<isNotNull prepend="," property="opr_user_id">OPR_USER_ID</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="opr_note">OPR_NOTE</isNotNull>	
			<isNotNull prepend="," property="dept_id">DEPT_ID</isNotNull>	
			<isNotNull prepend="," property="dept_name">DEPT_NAME</isNotNull>	
			<isNotNull prepend="," property="opr_obj">OPR_OBJ</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="opr_his_id">#opr_his_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="opr_title">#opr_title:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="opr_user_id">#opr_user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="opr_note">#opr_note:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="dept_id">#dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_name">#dept_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="opr_obj">#opr_obj:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaMobileImpHis" parameterClass="KonkaMobileImpHis">
		update KONKA_MOBILE_IMP_HIS
		<dynamic prepend="set">
			<isNotNull prepend="," property="opr_his_id">OPR_HIS_ID = #opr_his_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="opr_title">OPR_TITLE = #opr_title:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="opr_user_id">OPR_USER_ID = #opr_user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="opr_note">OPR_NOTE = #opr_note:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_name">DEPT_NAME = #dept_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="opr_obj">OPR_OBJ = #opr_obj:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteKonkaMobileImpHis" parameterClass="KonkaMobileImpHis">
		delete from KONKA_MOBILE_IMP_HIS where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>