			<isNotNull prepend="," property="exchange_info">#exchange_info:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="insurance_way">#insurance_way:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="insurance_price">#insurance_price:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="insurance_state">#insurance_state:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="remark">#remark:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="exchange_state">#exchange_state:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="pd_store">#pd_store:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_detail_id">#order_detail_id:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
