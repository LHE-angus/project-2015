			<isNotNull prepend="," property="old_order_id">OLD_ORDER_ID = #old_order_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="new_order_id">NEW_ORDER_ID = #new_order_id:DECIMAL#</isNotNull>
