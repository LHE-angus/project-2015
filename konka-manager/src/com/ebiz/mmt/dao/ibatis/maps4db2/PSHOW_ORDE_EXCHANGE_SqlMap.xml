	<sql id="sf-pshowOrdeExchange">
		<isNotEmpty prepend=" and " property="id">t.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="old_order_id">t.OLD_ORDER_ID = #old_order_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="new_order_id">t.NEW_ORDER_ID = #new_order_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="exchange_price">t.EXCHANGE_PRICE = #exchange_price:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_user_id">t.OPR_USER_ID = #opr_user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="opr_user_real_name">t.OPR_USER_REAL_NAME = #opr_user_real_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="oper_date">t.OPER_DATE = #oper_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_exchange">t.IS_EXCHANGE = #is_exchange:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_step">t.PD_STEP = #pd_step:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="exchange_info">t.EXCHANGE_INFO = #exchange_info:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="insurance_way">t.INSURANCE_WAY = #insurance_way:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="insurance_price">t.INSURANCE_PRICE = #insurance_price:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="insurance_state">t.INSURANCE_STATE = #insurance_state:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="remark">t.REMARK = #remark:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="exchange_state">t.EXCHANGE_STATE = #exchange_state:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="pd_store">t.PD_STORE = #pd_store:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_detail_id">t.ORDER_DETAIL_ID = #order_detail_id:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.trade_index_like">
		t.OLD_ORDER_ID in (select id from pshow_order where TRADE_INDEX like '%'|| #map.trade_index_like:VARCHAR# ||'%')
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.oper_date_start"><![CDATA[ t.OPER_DATE >= to_date(#map.oper_date_start#,'yyyy-MM-dd hh24:mi:ss') ]]></isNotEmpty>
		<isNotEmpty prepend=" and " property="map.oper_date_end"><![CDATA[ t.OPER_DATE <= to_date(#map.oper_date_end#,'yyyy-MM-dd hh24:mi:ss') ]]></isNotEmpty>
	  
	</sql>

	<select id="selectPshowOrdeExchangeListByOldOrderId" resultMap="pshowOrdeExchangeResult" parameterClass="long">
		select * from PSHOW_ORDE_EXCHANGE t where OLD_ORDER_ID = #value:DECIMAL#
	</select>

	<select id="selectPshowOrdeExchange" resultMap="pshowOrdeExchangeResult" parameterClass="pshowOrdeExchange" cacheModel="oneDayCache">
		select * from PSHOW_ORDE_EXCHANGE t where 1 = 1
		<include refid="sf-pshowOrdeExchange" />
	</select>

	<select id="selectPshowOrdeExchangeList" resultMap="pshowOrdeExchangeResultForList" parameterClass="pshowOrdeExchange" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from PSHOW_ORDE_EXCHANGE t where 1 = 1
		<include refid="sf-pshowOrdeExchange" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectPshowOrdeExchangeCount" resultClass="long" parameterClass="pshowOrdeExchange" cacheModel="oneDayCache">
		select count(*) from PSHOW_ORDE_EXCHANGE t where 1 = 1
		<include refid="sf-pshowOrdeExchange" />
	</select>

	<select id="selectPshowOrdeExchangePaginatedList" resultMap="pshowOrdeExchangeResult" parameterClass="pshowOrdeExchange" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from PSHOW_ORDE_EXCHANGE t where 1 = 1
		<include refid="sf-pshowOrdeExchange" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

