<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="EC_LUCKY_TIME">

	<typeAlias alias="ecLuckyTime" type="com.ebiz.mmt.domain.EcLuckyTime" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertEcLuckyTime" />
		<flushOnExecute statement="updateEcLuckyTime" />
		<flushOnExecute statement="deleteEcLuckyTime" />
	</cacheModel>

	<resultMap id="ecLuckyTimeResultForList" class="ecLuckyTime">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="LUCKY_ID" property="lucky_id" jdbcType="DECIMAL" />
		<result column="START_DATE" property="start_date" jdbcType="TIMESTAMP" />
		<result column="END_DATE" property="end_date" jdbcType="TIMESTAMP" />
	</resultMap>

	<resultMap id="ecLuckyTimeResult" class="ecLuckyTime" extends="ecLuckyTimeResultForList">
	</resultMap>

	<sql id="sf-ecLuckyTime">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="lucky_id">LUCKY_ID = #lucky_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="start_date">START_DATE = #start_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="end_date">END_DATE = #end_date:TIMESTAMP#</isNotEmpty>
	</sql>

	<select id="selectEcLuckyTime" resultMap="ecLuckyTimeResult" parameterClass="ecLuckyTime" cacheModel="oneDayCache">
		select * from EC_LUCKY_TIME where 1 = 1
		<include refid="sf-ecLuckyTime" />
	</select>

	<select id="selectEcLuckyTimeList" resultMap="ecLuckyTimeResultForList" parameterClass="ecLuckyTime" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from EC_LUCKY_TIME where 1 = 1
		<include refid="sf-ecLuckyTime" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectEcLuckyTimeCount" resultClass="long" parameterClass="ecLuckyTime" cacheModel="oneDayCache">
		select count(*) from EC_LUCKY_TIME where 1 = 1
		<include refid="sf-ecLuckyTime" />
	</select>

	<select id="selectEcLuckyTimePaginatedList" resultMap="ecLuckyTimeResult" parameterClass="ecLuckyTime" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from EC_LUCKY_TIME where 1 = 1
		<include refid="sf-ecLuckyTime" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertEcLuckyTime" parameterClass="ecLuckyTime">
		<selectKey resultClass="long" keyProperty="id">select seq_xxx.nextval as id from dual </selectKey>
		<![CDATA[insert into EC_LUCKY_TIME (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="lucky_id">LUCKY_ID</isNotNull>	
			<isNotNull prepend="," property="start_date">START_DATE</isNotNull>	
			<isNotNull prepend="," property="end_date">END_DATE</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="lucky_id">#lucky_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="start_date">#start_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="end_date">#end_date:TIMESTAMP#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateEcLuckyTime" parameterClass="EcLuckyTime">
		update EC_LUCKY_TIME
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="lucky_id">LUCKY_ID = #lucky_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="start_date">START_DATE = #start_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="end_date">END_DATE = #end_date:TIMESTAMP#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteEcLuckyTime" parameterClass="EcLuckyTime">
		delete from EC_LUCKY_TIME where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>