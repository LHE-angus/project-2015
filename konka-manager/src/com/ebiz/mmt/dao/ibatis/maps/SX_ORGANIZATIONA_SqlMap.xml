<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

	<!-- @author Xu,ZhengYang -->
	<!-- @version 2010-11-14 05:05:22 -->
<sqlMap namespace="ORGANIZATIONA">

	<typeAlias alias="organizationa" type="com.ebiz.mmt.domain.Organizationa" />

	
	
	<resultMap id="organizationaResultForList" class="organizationa">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="ORG_NAME" property="org_name" jdbcType="VARCHAR" />
		<result column="ORG_CODE" property="org_code" jdbcType="VARCHAR" />
		<result column="ORG_TEL" property="org_tel" jdbcType="VARCHAR" />
		<result column="ORG_FAX" property="org_fax" jdbcType="VARCHAR" />
		<result column="PAR_ORG_ID" property="par_org_id" jdbcType="DECIMAL" />
		<result column="ORG_HEADS_ID" property="org_heads_id" jdbcType="DECIMAL" />
		<result column="ORDER_VALUE" property="order_value" jdbcType="DECIMAL" />
		<result column="IS_DEL" property="is_del" jdbcType="DECIMAL" />
	</resultMap>
	
	<resultMap id="organizationaResult" class="organizationa" extends="organizationaResultForList">
	</resultMap>
	
	<resultMap id="organizationaResultList" class="organizationa" extends="organizationaResultForList">
	    <result column="LEVEL" property="map.level" jdbcType="DECIMAL" />
	</resultMap>
	
	<sql id="sf-organizationa">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="org_name">ORG_NAME = #org_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="org_code">ORG_CODE = #org_code:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="org_tel">ORG_TEL = #org_tel:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="org_fax">ORG_FAX = #org_fax:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="par_org_id">PAR_ORG_ID = #par_org_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="org_heads_id">ORG_HEADS_ID = #org_heads_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.search_dept_name">(ORG_NAME like '%' || #map.search_dept_name:VARCHAR# || '%' or f_getspell(ORG_NAME) like '%' || #map.search_dept_name:VARCHAR# || '%' or f_getspell(ORG_NAME,1) like '%' || #map.search_dept_name:VARCHAR# || '%')</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.notin">
			ID not in
			<iterate close=")" open="(" conjunction="," property="map.notin">#map.notin[]#</iterate>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.isin">
			ID in
			<iterate close=")" open="(" conjunction="," property="map.isin">#map.isin[]#</iterate>
		</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.org_name_like">ORG_NAME like '%' || #map.org_name_like:VARCHAR# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.org_code_like">ORG_CODE like '%' || #map.org_code_like:VARCHAR# || '%'</isNotEmpty>
	</sql>
	
	<select id="selectOrganizationa" resultMap="organizationaResult" parameterClass="organizationa" >
		select * from KONKA_ORGANIZATIONA a where 1 = 1
		<include refid="sf-organizationa" />
	</select>

	<select id="selectOrganizationaList" resultMap="organizationaResultList" parameterClass="organizationa" >
		<isNotEmpty property="row.count">
		<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select a.*, level from KONKA_ORGANIZATIONA a where 1 = 1
		<include refid="sf-organizationa" />
		start with par_org_id = 0
			connect by prior id = par_org_id
		<isNotEmpty property="row.count">
		<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectOrganizationaCount" resultClass="long" parameterClass="organizationa" >
		select count(*) from KONKA_ORGANIZATIONA where 1 = 1
		<include refid="sf-organizationa" />
	</select>

	<select id="selectOrganizationaPaginatedList" resultMap="organizationaResultList" parameterClass="organizationa" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select a.*  from KONKA_ORGANIZATIONA a  where 1 = 1
		<include refid="sf-organizationa" />
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertOrganizationa" parameterClass="organizationa">
		<selectKey resultClass="long" keyProperty="id">select KONKA_S_BASE.nextval as id from dual</selectKey>
		<![CDATA[insert into KONKA_ORGANIZATIONA (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>
			<isNotNull prepend="," property="org_name">ORG_NAME</isNotNull>
			<isNotNull prepend="," property="org_code">ORG_CODE</isNotNull>
			<isNotNull prepend="," property="org_tel">ORG_TEL</isNotNull>
			<isNotNull prepend="," property="org_fax">ORG_FAX</isNotNull>
			<isNotNull prepend="," property="par_org_id">PAR_ORG_ID</isNotNull>
			<isNotNull prepend="," property="org_heads_id">ORG_HEADS_ID</isNotNull>
			<isNotNull prepend="," property="order_value">ORDER_VALUE</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="org_name">#org_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_code">#org_code:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_tel">#org_tel:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_fax">#org_fax:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="par_org_id">#par_org_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="org_heads_id">#org_heads_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">#order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>
	
	<update id="updateOrganizationa" parameterClass="organizationa">
		update KONKA_ORGANIZATIONA
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="org_name">ORG_NAME = #org_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_code">ORG_CODE = #org_code:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_tel">ORG_TEL = #org_tel:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="org_fax">ORG_FAX = #org_fax:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="par_org_id">PAR_ORG_ID = #par_org_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="org_heads_id">ORG_HEADS_ID = #org_heads_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="order_value">ORDER_VALUE = #order_value:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
	<!-- 	<isNotEmpty prepend=" " property="par_org_id">PAR_ORG_ID = #par_org_id:DECIMAL#</isNotEmpty> -->
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
			ID in
			<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
		<!--  	or par_org_id in
			<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>-->
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteOrganizationa" parameterClass="organizationa">
		delete from KONKA_ORGANIZATIONA where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
			ID in
			<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>