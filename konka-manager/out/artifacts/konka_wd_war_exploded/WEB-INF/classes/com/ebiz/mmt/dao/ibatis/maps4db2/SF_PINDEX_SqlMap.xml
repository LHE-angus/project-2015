<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="SF_PINDEX">

	<typeAlias alias="sfPindex" type="com.ebiz.mmt.domain.SfPindex" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertSfPindex" />
		<flushOnExecute statement="updateSfPindex" />
		<flushOnExecute statement="deleteSfPindex" />
	</cacheModel>

	<resultMap id="sfPindexResultForList" class="sfPindex">
		<result column="PROVINCE" property="province" jdbcType="VARCHAR" />
		<result column="CITY" property="city" jdbcType="VARCHAR" />
		<result column="COUNTRY" property="country" jdbcType="VARCHAR" />
		<result column="TOWN" property="town" jdbcType="VARCHAR" />
		<result column="IS_REACHED" property="is_reached" jdbcType="VARCHAR" />
		<result column="PAR_INDEX" property="par_index" jdbcType="VARCHAR" />
		<result column="P_INDEX" property="p_index" jdbcType="VARCHAR" />
	</resultMap>

	<resultMap id="sfPindexResult" class="sfPindex" extends="sfPindexResultForList">
	</resultMap>

	<sql id="sf-sfPindex">
		<isNotEmpty prepend=" and " property="province">PROVINCE = #province:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="city">CITY = #city:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="country">COUNTRY = #country:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="town">TOWN = #town:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_reached">IS_REACHED = #is_reached:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="par_index">PAR_INDEX = #par_index:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="p_index">P_INDEX = #p_index:VARCHAR#</isNotEmpty>
	</sql>

	<select id="selectSfPindex" resultMap="sfPindexResult" parameterClass="sfPindex" cacheModel="oneDayCache">
		select * from SF_PINDEX where 1 = 1
		<include refid="sf-sfPindex" />
	</select>

	<select id="selectSfPindexList" resultMap="sfPindexResultForList" parameterClass="sfPindex" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from SF_PINDEX where 1 = 1
		<include refid="sf-sfPindex" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectSfPindexCount" resultClass="long" parameterClass="sfPindex" cacheModel="oneDayCache">
		select count(*) from SF_PINDEX where 1 = 1
		<include refid="sf-sfPindex" />
	</select>

	<select id="selectSfPindexPaginatedList" resultMap="sfPindexResult" parameterClass="sfPindex" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from SF_PINDEX where 1 = 1
		<include refid="sf-sfPindex" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertSfPindex" parameterClass="sfPindex">
		<![CDATA[insert into SF_PINDEX (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="province">PROVINCE</isNotNull>	
			<isNotNull prepend="," property="city">CITY</isNotNull>	
			<isNotNull prepend="," property="country">COUNTRY</isNotNull>	
			<isNotNull prepend="," property="town">TOWN</isNotNull>	
			<isNotNull prepend="," property="is_reached">IS_REACHED</isNotNull>	
			<isNotNull prepend="," property="par_index">PAR_INDEX</isNotNull>	
			<isNotNull prepend="," property="p_index">P_INDEX</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="province">#province:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="city">#city:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="country">#country:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="town">#town:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_reached">#is_reached:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="par_index">#par_index:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="p_index">#p_index:VARCHAR#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateSfPindex" parameterClass="SfPindex">
		update SF_PINDEX
		<dynamic prepend="set">
			<isNotNull prepend="," property="province">PROVINCE = #province:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="city">CITY = #city:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="country">COUNTRY = #country:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="town">TOWN = #town:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_reached">IS_REACHED = #is_reached:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="par_index">PAR_INDEX = #par_index:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="p_index">P_INDEX = #p_index:VARCHAR#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteSfPindex" parameterClass="SfPindex">
		delete from SF_PINDEX where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>