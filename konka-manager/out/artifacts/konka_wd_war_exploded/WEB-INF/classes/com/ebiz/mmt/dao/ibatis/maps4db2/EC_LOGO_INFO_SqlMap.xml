<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="EC_LOGO_INFO">

	<typeAlias alias="ecLogoInfo" type="com.ebiz.mmt.domain.EcLogoInfo" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertEcLogoInfo" />
		<flushOnExecute statement="updateEcLogoInfo" />
		<flushOnExecute statement="deleteEcLogoInfo" />
	</cacheModel>

	<resultMap id="ecLogoInfoResultForList" class="ecLogoInfo">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="TITLE" property="title" jdbcType="VARCHAR" />
		<result column="LOGO_URL" property="logo_url" jdbcType="VARCHAR" />
		<result column="OWN_SYS" property="own_sys" jdbcType="DECIMAL" />
		<result column="PLAT_SYS" property="plat_sys" jdbcType="DECIMAL" />
		<result column="DEPT_ID" property="dept_id" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="TIMESTAMP" />
