<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="J_STOCKS">

	<typeAlias alias="jStocks" type="com.ebiz.mmt.domain.JStocks" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertJStocks" />
		<flushOnExecute statement="updateJStocks" />
		<flushOnExecute statement="deleteJStocks" />
	</cacheModel>

	<resultMap id="jStocksResultForList" class="jStocks">
		<result column="STORE_ID" property="store_id" jdbcType="DECIMAL" />
		<result column="GOODS_ID" property="goods_id" jdbcType="DECIMAL" />
		<result column="STOCKS" property="stocks" jdbcType="DECIMAL" />
		<result column="TOTAL_COST" property="total_cost" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="jStocksResult" class="jStocks" extends="jStocksResultForList">
	</resultMap>

	<sql id="sf-jStocks">
		<isNotEmpty prepend=" and " property="store_id">STORE_ID = #store_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="goods_id">GOODS_ID = #goods_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="stocks">STOCKS = #stocks:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="total_cost">TOTAL_COST = #total_cost:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectJStocks" resultMap="jStocksResult" parameterClass="jStocks" cacheModel="oneDayCache">
		select * from J_STOCKS where 1 = 1
		<include refid="sf-jStocks" />
	</select>

	<select id="selectJStocksList" resultMap="jStocksResultForList" parameterClass="jStocks" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from J_STOCKS where 1 = 1
		<include refid="sf-jStocks" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectJStocksCount" resultClass="long" parameterClass="jStocks" cacheModel="oneDayCache">
		select count(*) from J_STOCKS where 1 = 1
		<include refid="sf-jStocks" />
	</select>

	<select id="selectJStocksPaginatedList" resultMap="jStocksResult" parameterClass="jStocks" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from J_STOCKS where 1 = 1
		<include refid="sf-jStocks" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertJStocks" parameterClass="jStocks">
		<!--<selectKey resultClass="long" keyProperty="id">select SEQ_J_BASE.nextval as id from dual </selectKey>-->
		<![CDATA[insert into J_STOCKS (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="store_id">STORE_ID</isNotNull>	
			<isNotNull prepend="," property="goods_id">GOODS_ID</isNotNull>	
			<isNotNull prepend="," property="stocks">STOCKS</isNotNull>	
			<isNotNull prepend="," property="total_cost">TOTAL_COST</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="store_id">#store_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="goods_id">#goods_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="stocks">#stocks:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="total_cost">#total_cost:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateJStocks" parameterClass="JStocks">
		update J_STOCKS
		<dynamic prepend="set">
			<isNotNull prepend="," property="store_id">STORE_ID = #store_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="goods_id">GOODS_ID = #goods_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="stocks">STOCKS = #stocks:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="total_cost">TOTAL_COST = #total_cost:DECIMAL#</isNotNull>
		</dynamic>
		where
		<!--<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>-->
		<isNotEmpty prepend=" " property="store_id">
			<isNotEmpty prepend=" " property="goods_id">STORE_ID = #store_id:DECIMAL# and GOODS_ID = #goods_id:DECIMAL#</isNotEmpty>
		</isNotEmpty>
	</update>

	<delete id="deleteJStocks" parameterClass="JStocks">
		delete from J_STOCKS where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>