	<resultMap id="userChangeInfoResult" class="userChangeInfo" extends="userChangeInfoResultForList">
	</resultMap>

	<sql id="sf-userChangeInfo">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="job_id">JOB_ID = #job_id:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_name_old">USER_NAME_OLD = #user_name_old:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="real_name_old">REAL_NAME_OLD = #real_name_old:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_id">USER_ID = #user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="start_date">START_DATE = #start_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="end_date">END_DATE = #end_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_name_new">USER_NAME_NEW = #user_name_new:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="real_name_new">REAL_NAME_NEW = #real_name_new:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="chan_status">CHAN_STATUS = #chan_status:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectUserChangeInfo" resultMap="userChangeInfoResult" parameterClass="userChangeInfo" cacheModel="oneDayCache">
		select * from USER_CHANGE_INFO where 1 = 1
		<include refid="sf-userChangeInfo" />
	</select>

	<select id="selectUserChangeInfoList" resultMap="userChangeInfoResultForList" parameterClass="userChangeInfo" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from USER_CHANGE_INFO where 1 = 1
		<include refid="sf-userChangeInfo" />
		 order by JOB_ID asc, START_DATE desc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectUserChangeInfoByTimeList" resultMap="userChangeInfoResultForList" parameterClass="userChangeInfo" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from USER_CHANGE_INFO where 1 = 1
		<include refid="sf-userChangeInfo" />
		 order by START_DATE desc 
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectUserChangeInfoCount" resultClass="long" parameterClass="userChangeInfo" cacheModel="oneDayCache">
		select count(*) from USER_CHANGE_INFO where 1 = 1
		<include refid="sf-userChangeInfo" />
	</select>

	<select id="selectUserChangeInfoPaginatedList" resultMap="userChangeInfoResult" parameterClass="userChangeInfo" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from USER_CHANGE_INFO where 1 = 1
		<include refid="sf-userChangeInfo" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertUserChangeInfo" parameterClass="userChangeInfo">
		<selectKey resultClass="long" keyProperty="id">select KONKA_R3_STORE_SEQ.NEXTVAL as id from dual </selectKey>
		<![CDATA[insert into USER_CHANGE_INFO (]]>
