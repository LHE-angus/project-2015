			<isNotNull prepend="," property="end_date">END_DATE</isNotNull>	
			<isNotNull prepend="," property="user_name_new">USER_NAME_NEW</isNotNull>	
			<isNotNull prepend="," property="real_name_new">REAL_NAME_NEW</isNotNull>
			<isNotNull prepend="," property="chan_status">CHAN_STATUS</isNotNull>		
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="job_id">#job_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="user_name_old">#user_name_old:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="real_name_old">#real_name_old:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="user_id">#user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_id">#dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="start_date">#start_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="end_date">#end_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="user_name_new">#user_name_new:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="real_name_new">#real_name_new:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="chan_status">#chan_status:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateUserChangeInfo" parameterClass="UserChangeInfo">
		update USER_CHANGE_INFO
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="job_id">JOB_ID = #job_id:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="user_name_old">USER_NAME_OLD = #user_name_old:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="real_name_old">REAL_NAME_OLD = #real_name_old:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="user_id">USER_ID = #user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="dept_id">DEPT_ID = #dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="start_date">START_DATE = #start_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="end_date">END_DATE = #end_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="user_name_new">USER_NAME_NEW = #user_name_new:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="real_name_new">REAL_NAME_NEW = #real_name_new:VARCHAR#</isNotNull>
		    <isNotNull prepend="," property="chan_status">CHAN_STATUS = #chan_status:VARCHAR#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteUserChangeInfo" parameterClass="UserChangeInfo">
		delete from USER_CHANGE_INFO where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>