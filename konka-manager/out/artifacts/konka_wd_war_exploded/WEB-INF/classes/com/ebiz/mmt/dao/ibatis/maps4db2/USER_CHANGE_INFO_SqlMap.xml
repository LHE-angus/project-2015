<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="USER_CHANGE_INFO">

	<typeAlias alias="userChangeInfo" type="com.ebiz.mmt.domain.UserChangeInfo" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertUserChangeInfo" />
		<flushOnExecute statement="updateUserChangeInfo" />
		<flushOnExecute statement="deleteUserChangeInfo" />
	</cacheModel>

	<resultMap id="userChangeInfoResultForList" class="userChangeInfo">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="JOB_ID" property="job_id" jdbcType="VARCHAR" />
		<result column="USER_NAME_OLD" property="user_name_old" jdbcType="VARCHAR" />
		<result column="REAL_NAME_OLD" property="real_name_old" jdbcType="VARCHAR" />
		<result column="USER_ID" property="user_id" jdbcType="DECIMAL" />
		<result column="DEPT_ID" property="dept_id" jdbcType="DECIMAL" />
		<result column="START_DATE" property="start_date" jdbcType="TIMESTAMP" />
		<result column="END_DATE" property="end_date" jdbcType="TIMESTAMP" />
		<result column="USER_NAME_NEW" property="user_name_new" jdbcType="VARCHAR" />
		<result column="REAL_NAME_NEW" property="real_name_new" jdbcType="VARCHAR" />
		<result column="CHAN_STATUS" property="chan_status" jdbcType="DECIMAL" />
