	<sql id="sf-sysObjData">
		<![CDATA[ and (t.create_date >=trunc(sysdate)) ]]>		
		<isNotEmpty prepend=" and " property="id">t.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="mod_id">t.MOD_ID = #mod_id:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="obj_name">t.OBJ_NAME = #obj_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="obj_content">t.OBJ_CONTENT = #obj_content:BLOB#</isNotEmpty>
		<isNotEmpty prepend=" and " property="obj_key">t.OBJ_KEY = #obj_key:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="create_date">t.CREATE_DATE = #create_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="obj_time">t.OBJ_TIME = #obj_time:DECIMAL#</isNotEmpty>
