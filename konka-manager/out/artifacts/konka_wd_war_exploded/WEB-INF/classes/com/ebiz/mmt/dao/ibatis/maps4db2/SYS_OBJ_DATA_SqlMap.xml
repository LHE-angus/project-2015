		<isNotEmpty prepend=" and " property="del_mark">t.DEL_MARK = #del_mark:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectSysObjData" resultMap="sysObjDataResult" parameterClass="sysObjData" cacheModel="oneDayCache">
		<![CDATA[ select t.*,(days(sysdate)-days(create_date)) as day from SYS_OBJ_DATA t where 1 = 1 ]]>
		<include refid="sf-sysObjData" />
	</select>

	<select id="selectSysObjDataList" resultMap="sysObjDataResultForList" parameterClass="sysObjData" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select t.*,(days(sysdate)-days(t.create_date)) as day from SYS_OBJ_DATA t where 1 = 1
		<include refid="sf-sysObjData" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) tt where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectSysObjDataCount" resultClass="long" parameterClass="sysObjData" cacheModel="oneDayCache">
		select count(*) from SYS_OBJ_DATA t where 1 = 1
		<include refid="sf-sysObjData" />
	</select>

	<select id="selectSysObjDataPaginatedList" resultMap="sysObjDataResult" parameterClass="sysObjData" cacheModel="oneDayCache">
		<![CDATA[ select tt.*,(days(sysdate)-days(create_date)) as day from ( select t_.*, rownum rn_ from ( ]]>
		select * from SYS_OBJ_DATA t where 1 = 1
		<include refid="sf-sysObjData" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) tt where rn_ >= (#row.first# + 1) ]]>
	</select>

