		<result column="OBJ_TIME" property="obj_time" jdbcType="DECIMAL" />
		<result column="REMARK" property="remark" jdbcType="VARCHAR" />
		<result column="DEL_MARK" property="del_mark" jdbcType="DECIMAL" />
