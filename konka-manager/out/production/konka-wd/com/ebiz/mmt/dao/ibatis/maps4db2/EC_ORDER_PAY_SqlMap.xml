	<select id="selectEcOrderPay" resultMap="ecOrderPayResult" parameterClass="ecOrderPay" cacheModel="oneDayCache">
		select * from EC_ORDER_PAY where 1 = 1
		<include refid="sf-ecOrderPay" />
	</select>

	<select id="selectEcOrderPayList" resultMap="ecOrderPayResultForList" parameterClass="ecOrderPay" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from EC_ORDER_PAY where 1 = 1
		<include refid="sf-ecOrderPay" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectEcOrderPayCount" resultClass="long" parameterClass="ecOrderPay" cacheModel="oneDayCache">
		select count(*) from EC_ORDER_PAY where 1 = 1
		<include refid="sf-ecOrderPay" />
	</select>

	<select id="selectEcOrderPayPaginatedList" resultMap="ecOrderPayResult" parameterClass="ecOrderPay" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from EC_ORDER_PAY where 1 = 1
		<include refid="sf-ecOrderPay" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertEcOrderPay" parameterClass="ecOrderPay">
