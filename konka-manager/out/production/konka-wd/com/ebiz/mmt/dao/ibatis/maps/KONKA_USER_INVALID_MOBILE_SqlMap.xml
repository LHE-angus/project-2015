<sqlMap namespace="KONKA_USER_INVALID_MOBILE">

	<typeAlias alias="konkaUserInvalidMobile" type="com.ebiz.mmt.domain.KonkaUserInvalidMobile" />

	

	<resultMap id="konkaUserInvalidMobileResultForList" class="konkaUserInvalidMobile">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="USER_ID" property="user_id" jdbcType="DECIMAL" />
		<result column="MOBILE_NO" property="mobile_no" jdbcType="VARCHAR" />

		<result column="YWY_CRC" property="ywy_crc" jdbcType="VARCHAR" />
		<result column="IS_ACTIVATE" property="is_activate" jdbcType="DECIMAL" />
		<result column="ENTP_CRC" property="entp_crc" jdbcType="VARCHAR" />
		<result column="SETPLAN_CRC" property="setplan_crc" jdbcType="VARCHAR" />
		<result column="USE_STATUS" property="use_status" jdbcType="DECIMAL" />
					
		<result column="CONFIRM_DATE" property="confirm_date" jdbcType="DATETIME" />		
		<result column="CREATE_DATE" property="create_date" jdbcType="DATETIME" />
		<result column="CREATE_UID" property="create_uid" jdbcType="DECIMAL" />
		<result column="UPDATE_DATE" property="update_date" jdbcType="DATETIME" />
		<result column="UPDATE_UID" property="update_uid" jdbcType="DECIMAL" />
	
		<result column="IS_DEL" property="is_del" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="konkaUserInvalidMobileResult" class="konkaUserInvalidMobile" extends="konkaUserInvalidMobileResultForList">
	</resultMap>

	<sql id="sf-konkaUserInvalidMobile">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_id">USER_ID = #user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="mobile_no">MOBILE_NO = #mobile_no:VARCHAR#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="ywy_crc">YWY_CRC = #ywy_crc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_activate">IS_ACTIVATE = #is_activate:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="entp_crc">ENTP_CRC = #entp_crc:VARCHAR#</isNotEmpty>		
		<isNotEmpty prepend=" and " property="setplan_crc">SETPLAN_CRC = #setplan_crc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="confirm_date">CONFIRM_DATE = #confirm_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="use_status">USE_STATUS = #use_status:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="create_date">CREATE_DATE = #create_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="create_uid">CREATE_UID = #create_uid:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_date">UPDATE_DATE = #update_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_uid">UPDATE_UID = #update_uid:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.ywy_name_like">	         
	        USER_ID in (select ID from KONKA_PE_PROD_USER where REAL_NAME like '%' || #map.ywy_name_like:VARCHAR# || '%')
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.ywy_phone">
	         MOBILE_NO = #map.ywy_phone:VARCHAR#
		</isNotEmpty>
		
	</sql>

	<select id="selectKonkaUserInvalidMobile" resultMap="konkaUserInvalidMobileResult" parameterClass="konkaUserInvalidMobile" >
		select * from KONKA_USER_INVALID_MOBILE where 1 = 1
		<include refid="sf-konkaUserInvalidMobile" />
	</select>

	<select id="selectKonkaUserInvalidMobileList" resultMap="konkaUserInvalidMobileResultForList" parameterClass="konkaUserInvalidMobile" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_USER_INVALID_MOBILE a where 1 = 1
		<include refid="sf-konkaUserInvalidMobile" />
		order by ID asc, MOBILE_NO asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>	
	</select>

	<select id="selectKonkaUserInvalidMobileCount" resultClass="long" parameterClass="konkaUserInvalidMobile" >
		select count(*) from KONKA_USER_INVALID_MOBILE where 1 = 1
		<include refid="sf-konkaUserInvalidMobile" />
	</select>

	<select id="selectKonkaUserInvalidMobilePaginatedList" resultMap="konkaUserInvalidMobileResult" parameterClass="konkaUserInvalidMobile" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
			select * from KONKA_USER_INVALID_MOBILE where 1=1
		<include refid="sf-konkaUserInvalidMobile" />
		    order by ID asc, MOBILE_NO asc
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>	
	</select>

	<insert id="insertKonkaUserInvalidMobile" parameterClass="konkaUserInvalidMobile">
	    <selectKey resultClass="long" keyProperty="id">select user_mobile_sequence.nextval as id from dual</selectKey>
		<![CDATA[insert into KONKA_USER_INVALID_MOBILE (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="user_id">USER_ID</isNotNull>	
			<isNotNull prepend="," property="mobile_no">MOBILE_NO</isNotNull>
				
			<isNotNull prepend="," property="ywy_crc">YWY_CRC</isNotNull>	
			<isNotNull prepend="," property="is_activate">IS_ACTIVATE</isNotNull>	
			<isNotNull prepend="," property="entp_crc">ENTP_CRC</isNotNull>
			<isNotNull prepend="," property="setplan_crc">SETPLAN_CRC</isNotNull>	
			<isNotNull prepend="," property="confirm_date">CONFIRM_DATE</isNotNull>	
			<isNotNull prepend="," property="use_status">USE_STATUS</isNotNull>

			<isNotNull prepend="," property="create_date">CREATE_DATE</isNotNull>	
			<isNotNull prepend="," property="create_uid">CREATE_UID</isNotNull>	
			<isNotNull prepend="," property="update_date">UPDATE_DATE</isNotNull>	
			<isNotNull prepend="," property="update_uid">UPDATE_UID</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>		
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_id">#user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="mobile_no">#mobile_no:VARCHAR#</isNotNull>
			
			<isNotNull prepend="," property="ywy_crc">#ywy_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_activate">#is_activate:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="entp_crc">#entp_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="setplan_crc">#setplan_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="confirm_date">#confirm_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="use_status">#use_status:DECIMAL#</isNotNull>
					
			<isNotNull prepend="," property="create_date">#create_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="create_uid">#create_uid:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="update_date">#update_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="update_uid">#update_uid:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">#is_del:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaUserInvalidMobile" parameterClass="konkaUserInvalidMobile">
		update KONKA_USER_INVALID_MOBILE
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_id">USER_ID = #user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="mobile_no">MOBILE_NO = #mobile_no:VARCHAR#</isNotNull>
			
			<isNotNull prepend="," property="ywy_crc">YWY_CRC = #ywy_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="is_activate">IS_ACTIVATE = #is_activate:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="entp_crc">ENTP_CRC = #entp_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="setplan_crc">SETPLAN_CRC = #setplan_crc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="confirm_date">CONFIRM_DATE = #confirm_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="use_status">USE_STATUS = #use_status:DECIMAL#</isNotNull>
				
			<isNotNull prepend="," property="create_date">CREATE_DATE = #create_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="create_uid">CREATE_UID = #create_uid:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="update_date">UPDATE_DATE = #update_date:DATETIME#</isNotNull>
			<isNotNull prepend="," property="update_uid">UPDATE_UID = #update_uid:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="is_del">IS_DEL = #is_del:DECIMAL#</isNotNull>
		</dynamic>
		where 
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<!-- 不传ID时，通过userid和mobile来更新 -->
		<isEmpty prepend=" " property="id">
			USER_ID = #user_id# and MOBILE_NO = #mobile_no:VARCHAR#
		</isEmpty>
	</update>

	<delete id="deleteKonkaUserInvalidMobile" parameterClass="konkaUserInvalidMobile">
		delete from KONKA_USER_INVALID_MOBILE where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<!--<isNotEmpty prepend=" " property="user_id">USER_ID = #user_id#</isNotEmpty>-->
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>