	<select id="selectKonkaSpActivityManagerList" resultMap="konkaSpActivityManagerResultListWithFiles" parameterClass="konkaSpActivityManager"
		cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select t.*,(select ty.hd_type from KONKA_SP_ACTIVITY_TYPE ty where ty.id=t.hd_id) map.hd_type from ( ]]>
		</isNotEmpty>
		select a.*,GET_FILES_SAVEPATH_FROM_KONKA_PE_ATTACHMENTS_BY_LINKID_AND_LINKTAB(a.id,'KONKA_SP_ACTIVITY_MANAGER') files from KONKA_SP_ACTIVITY_MANAGER a
		<isNotEmpty property="map.dept_id_start">
			left join MV_ORG_OF_CUSTOMER b
    		  on a.MAIN_R3_CODE = b.R3_CODE
		</isNotEmpty>
		<isNotEmpty property="map.filter_by_ywy_id_eq">
			left join MV_ORG_OF_CUSTOMER b
    		  on a.MAIN_R3_CODE = b.R3_CODE
		</isNotEmpty>
		where 1 = 1
		<isNotEmpty prepend=" and " property="map.dept_id_start">
				(b.cur_dept_id is not null and b.cur_dept_id in (
							select distinct dept_id from konka_dept start with dept_id in (
							SELECT DISTINCT (dept_id)
			  							FROM KONKA_ROLE_DATA_LEVEL
			 							WHERE role_id IN (SELECT role_id
			                     			FROM KONKA_PE_ROLE_USER
			                    			WHERE user_id = #map.session_user_id#)
							) or dept_id = #map.dept_id_start# 
							connect by prior dept_id = par_id 
						)
				)
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.filter_by_ywy_id_eq">
			b.user_id = #map.filter_by_ywy_id_eq#
		</isNotEmpty> 
		<include refid="sf-konkaSpActivityManager" />
		<!-- order by ID asc -->
		order by a.ADD_SP_DATE desc,a.ID DESC
		<isNotEmpty property="row.count">
			<![CDATA[ ) t where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

