<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="BASE_VISIT_TYPE">

	<typeAlias alias="baseVisitType" type="com.ebiz.mmt.domain.BaseVisitType" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertBaseVisitType" />
		<flushOnExecute statement="updateBaseVisitType" />
		<flushOnExecute statement="deleteBaseVisitType" />
	</cacheModel>

	<resultMap id="baseVisitTypeResultForList" class="baseVisitType">
		<result column="VISIT_TYPE_ID" property="visit_type_id" jdbcType="DECIMAL" />
		<result column="REPORT_TYPE" property="report_type" jdbcType="DECIMAL" />
		<result column="VISIT_TYPE_NAME" property="visit_type_name" jdbcType="VARCHAR" />
		<result column="ADD_ID" property="add_id" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="TIMESTAMP" />
		<result column="STATE" property="state" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="baseVisitTypeResult" class="baseVisitType" extends="baseVisitTypeResultForList">
	</resultMap>

	<sql id="sf-baseVisitType">
		<isNotEmpty prepend=" and " property="visit_type_id">VISIT_TYPE_ID = #visit_type_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="report_type">REPORT_TYPE = #report_type:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="visit_type_name">VISIT_TYPE_NAME = #visit_type_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_id">ADD_ID = #add_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="state">STATE = #state:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectBaseVisitType" resultMap="baseVisitTypeResult" parameterClass="baseVisitType" cacheModel="oneDayCache">
		select * from BASE_VISIT_TYPE where 1 = 1
		<include refid="sf-baseVisitType" />
		order by ADD_DATE
	</select>

	<select id="selectBaseVisitTypeList" resultMap="baseVisitTypeResultForList" parameterClass="baseVisitType" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from BASE_VISIT_TYPE where 1 = 1
		<include refid="sf-baseVisitType" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
		order by ADD_DATE
	</select>
    
    <select id="selectBaseVisitTypeByReportTypeList" resultMap="baseVisitTypeResultForList" parameterClass="baseVisitType" cacheModel="oneDayCache">
		select * from BASE_VISIT_TYPE where REPORT_TYPE = -1
       	<isNotEmpty prepend=" or " property="report_type">REPORT_TYPE = #report_type:DECIMAL#</isNotEmpty>
       	order by ADD_DATE
	</select>
	
	<select id="selectBaseVisitTypeCount" resultClass="long" parameterClass="baseVisitType" cacheModel="oneDayCache">
		select count(*) from BASE_VISIT_TYPE where 1 = 1
		<include refid="sf-baseVisitType" />
	</select>

	<select id="selectBaseVisitTypePaginatedList" resultMap="baseVisitTypeResult" parameterClass="baseVisitType" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from BASE_VISIT_TYPE where 1 = 1
		<include refid="sf-baseVisitType" />
		order by ADD_DATE
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertBaseVisitType" parameterClass="baseVisitType">
		<selectKey resultClass="long" keyProperty="visit_type_id">select SEQ_MOBILE_BUSINESS.nextval as visit_type_id from dual </selectKey>
		<![CDATA[insert into BASE_VISIT_TYPE (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="visit_type_id">VISIT_TYPE_ID</isNotNull>	
			<isNotNull prepend="," property="report_type">REPORT_TYPE</isNotNull>	
			<isNotNull prepend="," property="visit_type_name">VISIT_TYPE_NAME</isNotNull>	
			<isNotNull prepend="," property="add_id">ADD_ID</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="state">STATE</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="visit_type_id">#visit_type_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="report_type">#report_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="visit_type_name">#visit_type_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="add_id">#add_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">#add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="state">#state:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateBaseVisitType" parameterClass="BaseVisitType">
		update BASE_VISIT_TYPE
		<dynamic prepend="set">
			<isNotNull prepend="," property="visit_type_id">VISIT_TYPE_ID = #visit_type_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="report_type">REPORT_TYPE = #report_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="visit_type_name">VISIT_TYPE_NAME = #visit_type_name:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="add_id">ADD_ID = #add_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotNull>
			<isNotNull prepend="," property="state">STATE = #state:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="visit_type_id">VISIT_TYPE_ID = #visit_type_id#</isNotEmpty>
		<isEmpty prepend=" " property="visit_type_id">
			<isNotEmpty prepend=" " property="map.pks">
				VISIT_TYPE_ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteBaseVisitType" parameterClass="BaseVisitType">
		delete from BASE_VISIT_TYPE where
		<isNotEmpty prepend=" " property="visit_type_id">VISIT_TYPE_ID = #visit_type_id#</isNotEmpty>
		<isEmpty prepend=" " property="visit_type_id">
			<isNotEmpty prepend=" " property="map.pks">
				VISIT_TYPE_ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>