		<isNotEmpty prepend=" and " property="begin_date">BEGIN_DATE = #begin_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="end_date">END_DATE = #end_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="sale_num">SALE_NUM = #sale_num:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="sale_money">SALE_MONEY = #sale_money:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="memo">MEMO = #memo:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_name">ADD_USER_NAME = #add_user_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user">ADD_USER = #add_user:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">IS_DEL = #is_del:DECIMAL#</isNotEmpty>
	</sql>
	<sql id="sf-konkaFightActivityManager-a">
		<isNotEmpty prepend=" and " property="id">a.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="activity_name">a.ACTIVITY_NAME = #activity_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="store_id">a.STORE_ID = #store_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="brand_id">a.BRAND_ID = #brand_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="model">a.MODEL = #model:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="begin_date">a.BEGIN_DATE = #begin_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="end_date">a.END_DATE = #end_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="sale_num">a.SALE_NUM = #sale_num:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="sale_money">a.SALE_MONEY = #sale_money:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="memo">a.MEMO = #memo:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_name">a.ADD_USER_NAME = #add_user_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user">a.ADD_USER = #add_user:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">a.ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">a.IS_DEL = #is_del:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.model_like">a.MODEL LIKE '%' || #map.model_like:VARCHAR# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.customer_name_like">b.kh_name like '%' || #map.customer_name_like:VARCHAR# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.store_name_like">b.store_name like '%' || #map.store_name_like:VARCHAR# || '%'</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.r3_code">b.r3_code = #map.r3_code:VARCHAR# </isNotEmpty>
		<isNotEmpty prepend=" and " property="map.dept_id">b.dept_id = #map.dept_id:DECIMAL# </isNotEmpty>
		<isNotEmpty prepend=" and " property="map.add_date_begin">
		<![CDATA[a.add_date >= to_date(#map.add_date_start#,'yyyy-mm-dd HH24:mi:ss') ]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.add_date_end">
		<![CDATA[a.add_date <= to_date(#map.add_date_end#,'yyyy-mm-dd HH24:mi:ss') ]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.activity_name_like">a.activity_name like '%' || #map.activity_name_like# || '%'</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.begin_date_start">
		<![CDATA[a.begin_date >= to_date(#map.begin_date_start#,'yyyy-mm-dd HH24:mi:ss') ]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.begin_date_end">
		<![CDATA[a.end_date <= to_date(#map.begin_date_end#,'yyyy-mm-dd HH24:mi:ss') ]]>
		</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.dept_id_start">
		a.add_user in (	select DISTINCT id from konka_pe_prod_user where dept_id in (
						select dept_id from konka_dept start with dept_id in (
						SELECT DISTINCT (dept_id)
		  							FROM KONKA_ROLE_DATA_LEVEL
		 							WHERE role_id IN (SELECT role_id
		                     			FROM KONKA_PE_ROLE_USER
		                    			WHERE user_id = #map.session_user_id#)
						) or dept_id = #map.dept_id_start# 
						connect by prior dept_id = par_id 
					)
			)
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.filter_by_ywy_id_eq">
			<![CDATA[ (a.add_user = #map.filter_by_ywy_id_eq# )]]>
		</isNotEmpty> 
		<isNotEmpty prepend=" and " property="map.par_dept_id">
			a.add_user in (	select DISTINCT id from konka_pe_prod_user where dept_id in ( select dept_id from KONKA_DEPT start with dept_id = #map.par_dept_id:DECIMAL# connect by prior dept_id = par_id))
		</isNotEmpty>
	</sql>

	<select id="selectKonkaFightActivityManager" resultMap="konkaFightActivityManagerResult" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		select * from KONKA_FIGHT_ACTIVITY_MANAGER where 1 = 1
		<include refid="sf-konkaFightActivityManager" />
	</select>

	<select id="selectKonkaFightActivityManagerList" resultMap="konkaFightActivityManagerResultForList" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_FIGHT_ACTIVITY_MANAGER where 1 = 1
		<include refid="sf-konkaFightActivityManager" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>
	
	<select id="selectKonkaFightActivityManagerMainPaginatedList" resultMap="konkaFightActivityManagerResultForMainList" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		SELECT a.*,b.STORE_NAME,b.DEPT_NAME,B.R3_CODE,
		B.KH_NAME AS customer_name,pd.brand_name
			FROM
			    KONKA_FIGHT_ACTIVITY_MANAGER a
			LEFT JOIN konka_r3_store b
			ON    a.STORE_ID =b.STORE_ID
			LEFT JOIN(
	        SELECT	brand_id,brand_name
	        FROM      konka_mobile_pd
	        GROUP BY    brand_id ,     brand_name) pd
			ON	 a.brand_id=pd.brand_id
			WHERE			    1=1
		<include refid="sf-konkaFightActivityManager-a" />
		 order by a.ID desc 
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaFightActivityManagerMainCount" resultClass="long" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		SELECT count(*)
			FROM
			   KONKA_FIGHT_ACTIVITY_MANAGER a
			LEFT JOIN konka_r3_store b
			ON    a.STORE_ID =b.STORE_ID
			WHERE	1=1
		<include refid="sf-konkaFightActivityManager-a" />
	</select>
	<select id="selectKonkaFightActivityManagerCount" resultClass="long" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		select count(*) from KONKA_FIGHT_ACTIVITY_MANAGER where 1 = 1
		<include refid="sf-konkaFightActivityManager" />
	</select>

	<select id="selectKonkaFightActivityManagerPaginatedList" resultMap="konkaFightActivityManagerResult" parameterClass="konkaFightActivityManager" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from KONKA_FIGHT_ACTIVITY_MANAGER where 1 = 1
		<include refid="sf-konkaFightActivityManager" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

