	<!-- author Hu,hao 2013-12-06 -->
	<select id="selectKonkaR3ShopTaskToDeptUserCount" resultClass="long" parameterClass="konkaR3ShopTask" cacheModel="oneDayCache">
		select count(1) from KONKA_R3_SHOP_TASK a 
			 left join KONKA_DEPT b on a.DEPT_ID = b.DEPT_ID
 			 left join KONKA_PE_PROD_USER c on a.R_USER_ID = c.ID 
		where 1 = 1
		<include refid="sf-konkaR3ShopTask" />
	</select>
	
	<!-- author Hu,hao 2013-12-06 -->
	<select id="selectKonkaR3ShopTaskToDeptUserPaginatedList" resultMap="konkaR3ShopTaskToDeptUserResult" parameterClass="konkaR3ShopTask" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select a.*,b.dept_name,c.user_name as r_user_name from KONKA_R3_SHOP_TASK a 
			 left join KONKA_DEPT b on a.DEPT_ID = b.DEPT_ID
 			 left join KONKA_PE_PROD_USER c on a.R_USER_ID = c.ID 
		where 1 = 1
		<include refid="sf-konkaR3ShopTask" />
		
		order by dept_id asc,r3_code asc
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

