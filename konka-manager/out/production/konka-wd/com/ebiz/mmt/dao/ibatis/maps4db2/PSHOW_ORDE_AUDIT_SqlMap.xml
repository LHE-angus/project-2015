	</sql>

	<select id="selectPshowOrdeAudit" resultMap="pshowOrdeAuditResult" parameterClass="pshowOrdeAudit" cacheModel="oneDayCache">
		select * from PSHOW_ORDE_AUDIT where 1 = 1
		<include refid="sf-pshowOrdeAudit" />
	</select>

	<select id="selectPshowOrdeAuditList" resultMap="pshowOrdeAuditResultListForRealName" parameterClass="pshowOrdeAudit" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select t.*, b.REAL_NAME from PSHOW_ORDE_AUDIT t ,KONKA_PE_PROD_USER b where 1 = 1 and t.OPR_USER_ID = b.ID
		<include refid="sf-pshowOrdeAudit-t" />
		<isNotEmpty property="map.orderByDate">
			order by t.OPER_DATE
		</isNotEmpty>
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectPshowOrdeAuditCount" resultClass="long" parameterClass="pshowOrdeAudit" cacheModel="oneDayCache">
		select count(*) from PSHOW_ORDE_AUDIT where 1 = 1
		<include refid="sf-pshowOrdeAudit" />
	</select>

	<select id="selectPshowOrdeAuditPaginatedList" resultMap="pshowOrdeAuditResult" parameterClass="pshowOrdeAudit" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select * from PSHOW_ORDE_AUDIT where 1 = 1
		<include refid="sf-pshowOrdeAudit" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

