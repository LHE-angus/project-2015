			<isNotNull prepend="," property="order_id">ORDER_ID = #order_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="opr_user_id">OPR_USER_ID = #opr_user_id:DECIMAL#</isNotNull>
