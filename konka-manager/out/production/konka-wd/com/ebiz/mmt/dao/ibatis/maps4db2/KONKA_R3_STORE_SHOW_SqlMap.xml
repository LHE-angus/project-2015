	<select id="selectKonkaR3StoreShowLBCount" resultClass="long" parameterClass="konkaR3StoreShow" cacheModel="oneDayCache">
		SELECT count(*) FROM  KONKA_R3_STORE_SHOW store_show
			       LEFT JOIN
			          (SELECT store.STORE_ID,
			                  store.STORE_NAME,
			                  shop.R3_CODE,
			                  shop.CUSTOMER_NAME,
			                  cust_type.PAR_CUSTOMER_TYPE,
                              cust_type.PAR_CUSTOMER_TYPE_NAME
			             FROM    KONKA_R3_STORE store
			                  LEFT JOIN
			                     KONKA_R3_SHOP shop
			                  ON store.R3_CODE = shop.R3_CODE
			                  left join MV_ORG_OF_CUSTOMER cust_type
                              on store.R3_CODE=cust_type.R3_CODE) cust_message
			   ON store_show.STORE_ID = cust_message.STORE_ID
			   left join KONKA_PE_PROD_USER prod_user
               on store_show.ADD_USER_ID=prod_user.ID
			    where 1 = 1
			<include refid="sf-konkaR3StoreShowLB" />
	</select>
