		</isNotEmpty>
		select t1.*,t2.dept_name from JF_SCORTS_DETAILS t1
		left join KONKA_DEPT t2 on t2.dept_id = t1.dept_id
		where 1 = 1
		<include refid="sf-jfScortsDetails-t1" />
		<!-- order by ID asc -->
		<isNotEmpty prepend=" " property="map.add_date_asc">order by t1.ADD_DATE asc</isNotEmpty>
		<isNotEmpty property="row.count">
