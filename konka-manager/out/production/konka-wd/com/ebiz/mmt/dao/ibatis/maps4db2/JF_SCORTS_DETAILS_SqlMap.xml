		</isNotEmpty>
	</select>
	
	<select id="selectJfScortsDetailsAndDeptNameForMemberCardList" resultMap="jfScortsDetailsAndDeptNameResultForList" parameterClass="jfScortsDetails" cacheModel="oneDayCache">
