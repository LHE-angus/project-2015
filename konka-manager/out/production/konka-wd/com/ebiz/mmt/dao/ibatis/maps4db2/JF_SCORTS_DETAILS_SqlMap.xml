	<select id="selectJfScortsDetailsPaginatedList" resultMap="jfScortsDetailsResult" parameterClass="jfScortsDetails" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
