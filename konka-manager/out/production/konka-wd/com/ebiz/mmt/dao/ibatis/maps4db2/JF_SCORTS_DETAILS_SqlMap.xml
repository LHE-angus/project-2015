		select * from JF_SCORTS_DETAILS where 1 = 1
		<include refid="sf-jfScortsDetails" />
		<!-- order by ID asc -->
		<isNotEmpty prepend=" " property="map.add_date_asc">order by ADD_DATE asc</isNotEmpty>
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
