	<select id="selectKonkaParagonShowt" resultMap="konkaParagonShowtResultForView" parameterClass="konkaParagonShowt" >
	 select a.*,
       b.user_name as user_name,
       c.show_shop_name as show_shop_name,
       d.version_code,
       d.version_name
	  from KONKA_PARAGON_SHOWT a
	  left join KONKA_PE_PROD_USER b on a.addman = b.id
	                                and b.is_del = 0
	  left join konka_paragon_showinfo c on c.show_shop_code = a.show_shop_code
	  left join konka_paragon_showversion d on d.version_id = a.version_id
	 where 1 = 1
		<include refid="sf-konkaParagonShowt" />
	</select>
	
