<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

	<!-- @author Hui,Gang -->
	<!-- @version Build 2010-12-13 14:49:33 -->
<sqlMap namespace="FILES_PROPERTY">
	
	<typeAlias alias="filesProperty" type="com.ebiz.mmt.domain.KonkaoaFilesProperty" />

	

	<resultMap id="filesPropertyResult" class="filesProperty">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="LINK_ID" property="link_id" jdbcType="DECIMAL" />
		<result column="C_TYPE" property="c_type" jdbcType="DECIMAL" />
		<result column="C_INDEX" property="c_index" jdbcType="DECIMAL" />
		<result column="C_DESC" property="c_desc" jdbcType="VARCHAR" />
		<result column="AMOUNT" property="amount" jdbcType="DECIMAL" />
		<result column="COST" property="cost" jdbcType="DECIMAL" />
		<result column="ADD_USER_ID" property="add_user_id" jdbcType="DECIMAL" />
		<result column="ADD_DEPT_ID" property="add_dept_id" jdbcType="DECIMAL" />
		<result column="ADD_DATE" property="add_date" jdbcType="DATE" />
		<result column="ADD_TYPE" property="add_type" jdbcType="DECIMAL" />
	</resultMap>

	<sql id="sf-filesProperty">
		<isNotEmpty prepend=" and " property="id">a.ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="link_id">a.LINK_ID = #link_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_type">a.C_TYPE = #c_type:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_index">a.C_INDEX = #c_index:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="c_desc">a.C_DESC = #c_desc:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="amount">a.amount = #amount:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="cost">a.COST = #cost:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_id">a.ADD_USER_ID = #add_user_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_dept_id">a.ADD_DEPT_ID = #add_dept_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">a.ADD_DATE = #add_date:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_type">a.ADD_TYPE = #add_type:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectKonkaoaFilesProperty" resultMap="filesPropertyResult" parameterClass="filesProperty" >
		select a.* from KONKAOA_FILES_PROPERTY a where 1 = 1
		<include refid="sf-filesProperty" />
	</select>

	<select id="selectKonkaoaFilesPropertyList" resultMap="filesPropertyResult" parameterClass="filesProperty" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select a.* from KONKAOA_FILES_PROPERTY a where 1 = 1
		<include refid="sf-filesProperty" />
		<!-- 此外排序不要修改 -->
		order by ID asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaoaFilesPropertyCount" resultClass="long" parameterClass="filesProperty" >
		select count(*) from KONKAOA_FILES_PROPERTY where 1 = 1
		<include refid="sf-filesProperty" />
	</select>

	<select id="selectKonkaoaFilesPropertyPaginatedList" resultMap="filesPropertyResult" parameterClass="filesProperty" >
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select a.* from KONKAOA_FILES_PROPERTY a where 1 = 1
		<include refid="sf-filesProperty" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertKonkaoaFilesProperty" parameterClass="filesProperty">
		<selectKey resultClass="long" keyProperty="id">select KONKA_ARTICLE_ID_SEQ.nextval as id from dual</selectKey>
		<![CDATA[insert into KONKAOA_FILES_PROPERTY (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="link_id">LINK_ID</isNotNull>	
			<isNotNull prepend="," property="c_type">C_TYPE</isNotNull>	
			<isNotNull prepend="," property="c_index">C_INDEX</isNotNull>	
			<isNotNull prepend="," property="c_desc">C_DESC</isNotNull>	
			<isNotNull prepend="," property="amount">AMOUNT</isNotNull>	
			<isNotNull prepend="," property="cost">COST</isNotNull>	
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID</isNotNull>	
			<isNotNull prepend="," property="add_dept_id">ADD_DEPT_ID</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="add_type">ADD_TYPE</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="link_id">#link_id:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="c_type">#c_type:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="c_index">#c_index:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="c_desc">#c_desc:VARCHAR#</isNotNull>	
			<isNotNull prepend="," property="amount">#amount:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="cost">#cost:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="add_user_id">#add_user_id:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="add_dept_id">#add_dept_id:DECIMAL#</isNotNull>	
			<isNotNull prepend="," property="add_date">#add_date:DATE#</isNotNull>	
			<isNotNull prepend="," property="add_type">#add_type:DECIMAL#</isNotNull>	
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaoaFilesProperty" parameterClass="filesProperty">
		update KONKAOA_FILES_PROPERTY
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="link_id">LINK_ID = #link_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_type">C_TYPE = #c_type:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_index">C_INDEX = #c_index:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="c_desc">C_DESC = #c_desc:VARCHAR#</isNotNull>
			<isNotNull prepend="," property="amount">AMOUNT = #amount:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="cost">COST = #cost:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_user_id">ADD_USER_ID = #add_user_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_dept_id">ADD_DEPT_ID = #add_dept_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_date">ADD_DATE = #add_date:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="add_type">ADD_TYPE = #add_type:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteKonkaoaFilesProperty" parameterClass="filesProperty">
		delete from KONKAOA_FILES_PROPERTY where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
		<isNotEmpty prepend=" " property="link_id">LINK_ID = #link_id#</isNotEmpty>
	</delete>

</sqlMap>