	<insert id="insertKonkaOrderInfoTransDetails" parameterClass="konkaOrderInfoTransDetails">
		<selectKey resultClass="long" keyProperty="ensu_id">select SEQ_KONKA_ORDER_INFO_TRANS.nextval as ensu_id from dual </selectKey>
		<![CDATA[insert into KONKA_ORDER_INFO_TRANS_DETAILS (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="ensu_id">ENSU_ID</isNotNull>	
			<isNotNull prepend="," property="order_id">ORDER_ID</isNotNull>	
			<isNotNull prepend="," property="trans_id">TRANS_ID</isNotNull>	
			<isNotNull prepend="," property="trade_index">TRADE_INDEX</isNotNull>	
			<isNotNull prepend="," property="trans_index_detail">TRANS_INDEX_DETAIL</isNotNull>	
			<isNotNull prepend="," property="r3_id">R3_ID</isNotNull>	
			<isNotNull prepend="," property="r3_vbedl">R3_VBEDL</isNotNull>	
			<isNotNull prepend="," property="model_name">MODEL_NAME</isNotNull>	
			<isNotNull prepend="," property="model_num">MODEL_NUM</isNotNull>	
			<isNotNull prepend="," property="model_money">MODEL_MONEY</isNotNull>	
			<isNotNull prepend="," property="trans_num">TRANS_NUM</isNotNull>	
			<isNotNull prepend="," property="trans_money">TRANS_MONEY</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_status">TRANS_ENSU_STATUS</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_num">TRANS_ENSU_NUM</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_date">TRANS_ENSU_DATE</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_user">TRANS_ENSU_USER</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_desc">TRANS_ENSU_DESC</isNotNull>	
			<isNotNull prepend="," property="trans_ensured_num">TRANS_ENSURED_NUM</isNotNull>	
			<isNotNull prepend="," property="add_date">ADD_DATE</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_user_phone">TRANS_ENSU_USER_PHONE</isNotNull>	
			<isNotNull prepend="," property="r3_code">R3_CODE</isNotNull>	
			<isNotNull prepend="," property="trans_recl_addr">TRANS_RECL_ADDR</isNotNull>	
			<isNotNull prepend="," property="trans_recl_user">TRANS_RECL_USER</isNotNull>	
			<isNotNull prepend="," property="trans_recl_user_phone">TRANS_RECL_USER_PHONE</isNotNull>	
			<isNotNull prepend="," property="is_print">IS_PRINT</isNotNull>	
			<isNotNull prepend="," property="is_del">IS_DEL</isNotNull>	
			<isNotNull prepend="," property="print_user">PRINT_USER</isNotNull>	
			<isNotNull prepend="," property="r3_code_sdf">R3_CODE_SDF</isNotNull>	
			<isNotNull prepend="," property="trans_ensu_type">TRANS_ENSU_TYPE</isNotNull>	
			<isNotNull prepend="," property="trans_other_ensu_user">TRANS_OTHER_ENSU_USER</isNotNull>	
			<isNotNull prepend="," property="trans_detail_status">TRANS_DETAIL_STATUS</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
