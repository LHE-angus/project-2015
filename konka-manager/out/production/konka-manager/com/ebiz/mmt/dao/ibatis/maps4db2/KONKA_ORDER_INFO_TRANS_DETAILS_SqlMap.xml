	<select id="selectKonkaOrderInfoTransDetailsPaginatedcounttzd" resultClass="long" parameterClass="konkaOrderInfoTransDetails" cacheModel="oneDayCache">
			select count(*)
			from konka_r3_zsof krz
          	left join KONKA_ORDER_INFO koi on koi.R3_ID = krz.VBELN
          	<![CDATA[      where 1=1  and krz.lfdat > sysdate -30 ]]>
		<include refid="sf-konkaOrderInfoTransDetails_tzd" />
	</select>	
	<select id="selectKonkaOrderInfoTransDetailsPaginatedListtzd" resultMap="konkaOrderInfoTransDetailsResultForListtzd" parameterClass="konkaOrderInfoTransDetails" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		 select krz.*,
                  koi.TRADE_INDEX,
                  koi.WE,         
                  COALESCE ((select sum(koid.GOOD_COUNT) from konka_order_info_details koid where koid.ORDER_ID = koi.ID),0) good_count,
                  COALESCE ((select sum(koid.GOOD_SUM_PRICE) from konka_order_info_details koid where koid.ORDER_ID = koi.ID),0) good_sum_price,
                  COALESCE ((select sum(koitd.TRANS_NUM) from KONKA_ORDER_INFO_TRANS_DETAILS koitd where koitd.is_del=0 and koitd.R3_VBEDL = krz.VBEDL),0) trans_num,
                  (SELECT koit.trans_date FROM KONKA_ORDER_INFO_TRANS koit WHERE koit.is_del=0 and koit.TRANS_ID IN (SELECT koitd.trans_id FROM konka_order_info_trans_details koitd
                                    WHERE koitd.is_del=0 AND koitd.R3_ID = krz.VBELN
                                   ORDER BY koitd.ENSU_ID DESC
                                   FETCH FIRST 1 ROWS ONLY))
                          trans_date,
                  (select count(1) from KONKA_ORDER_INFO_TRANS koit where koit.is_del=0 and koit.TRANS_ID in
                  (select distinct koitd.TRANS_ID from konka_order_info_trans_details koitd where koitd.is_del=0 and koitd.R3_VBEDL = krz.VBEDL )) trans_sum_nums,
                  koi.SEND_TYPE,
                  koi.USER_NAME,
                  koi.USER_PHONE,
                  koi.USER_ADDR
          from konka_r3_zsof krz
          left join KONKA_ORDER_INFO koi on koi.R3_ID = krz.VBELN
    <![CDATA[      where 1=1  and krz.lfdat > sysdate -30 ]]>
          <include refid="sf-konkaOrderInfoTransDetails_tzd" />
          order by krz.erdat desc
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>
	
