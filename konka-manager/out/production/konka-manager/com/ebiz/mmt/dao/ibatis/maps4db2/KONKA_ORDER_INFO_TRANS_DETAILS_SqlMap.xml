	<sql id="sf-konkaOrderInfoTransDetails_tzd">
		<isNotEmpty prepend=" and " property="map.r3_vbedl_like">
			krz.vbedl like '%' || #map.r3_vbedl_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.vbeln_like">
			krz.vbeln like '%' || #map.vbeln_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.customer_name_like">
			krz.name1 like '%' || #map.customer_name_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.r3_code_like">
			krz.kunnr like '%' || #map.r3_code_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.trade_index_like">
			koi.trade_index like '%' || #map.trade_index_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.trans_index_like">
			<![CDATA[krz.VBEDL in (select distinct koitd.R3_VBEDL from KONKA_ORDER_INFO_TRANS_DETAILS koitd where koitd.TRANS_ID in(
                       select koit.TRANS_ID from konka_order_info_trans koit where koit.TRANS_INDEX like '%' || #map.trans_index_like:VARCHAR# || '%' ))]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.r3_code_sdf_like">
			koi.we like '%' || #map.r3_code_sdf_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.user_name_like">
			koi.user_name like '%' || #map.user_name_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.user_phone_like">
			koi.user_phone like '%' || #map.user_phone_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.user_addr_like">
			koi.user_addr like '%' || #map.user_addr_like:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.send_type">
			koi.send_type = #map.send_type:DECIMAL#
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.trans_date_s">
			<![CDATA[krz.VBEDL in (select koitd.R3_VBEDL from KONKA_ORDER_INFO_TRANS_DETAILS koitd where koitd.TRANS_ID in(
                       select koit.TRANS_ID from konka_order_info_trans koit where koit.TRANS_DATE >= to_date(#map.trans_date_s#,'yyyy-MM-dd hh24:mi:ss')))]]>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.trans_date_e">
			<![CDATA[krz.VBEDL in (select koitd.R3_VBEDL from KONKA_ORDER_INFO_TRANS_DETAILS koitd where koitd.TRANS_ID in(
                       select koit.TRANS_ID from konka_order_info_trans koit where koit.TRANS_DATE <= to_date(#map.trans_date_e#,'yyyy-MM-dd hh24:mi:ss')))]]>
		</isNotEmpty>
		
		<!-- 企业端查询  start-->
		<isNotEmpty prepend=" and " property="map.cust_id">
			koi.CUST_ID = #map.cust_id:DECIMAL#
		</isNotEmpty>
		<!-- 企业端查询 end-->
		
		<!-- 管理端查询 start-->
		<isNotEmpty prepend=" and " property="map.par_dept_id">
			koi.add_dept_id in 
  				( select dept_id from KONKA_DEPT start with dept_id = #map.par_dept_id:DECIMAL# connect by prior dept_id = par_id)
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.par_or_children_dept_id">
			(
			 koi.add_dept_id in 
			 	( select dept_id from KONKA_DEPT start with dept_id = #map.par_or_children_dept_id:DECIMAL# connect by prior dept_id = par_id
								union 
							   select dept_id from KONKA_DEPT start with dept_id = #map.par_or_children_dept_id:DECIMAL# connect by prior par_id = dept_id)
		    <isNotEmpty prepend=" or " property="map.session_user_id">
			 koi.add_dept_id in 
			     (select dept_id from konka_dept start with dept_id in (
								SELECT DISTINCT (dept_id)
				  							FROM KONKA_ROLE_DATA_LEVEL
				 							WHERE role_id IN (SELECT role_id
				                     			FROM KONKA_PE_ROLE_USER
				                    			WHERE user_id = #map.session_user_id#)
								) 
								connect by prior dept_id = par_id )
			</isNotEmpty>
			)
		</isNotEmpty>
		<!-- 管理端查询 end-->
	</sql>

