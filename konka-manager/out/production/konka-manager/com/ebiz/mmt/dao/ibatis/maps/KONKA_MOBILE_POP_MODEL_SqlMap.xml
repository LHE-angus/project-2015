<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="KONKA_MOBILE_POP_MODEL">

	<typeAlias alias="konkaMobilePopModel" type="com.ebiz.mmt.domain.KonkaMobilePopModel" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertKonkaMobilePopModel" />
		<flushOnExecute statement="updateKonkaMobilePopModel" />
		<flushOnExecute statement="deleteKonkaMobilePopModel" />
	</cacheModel>

	<resultMap id="konkaMobilePopModelResultForList" class="konkaMobilePopModel">
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="MODEL_ID" property="model_id" jdbcType="DECIMAL" />
		<result column="USER_ID" property="user_id" jdbcType="DECIMAL" />
	</resultMap>

	<resultMap id="konkaMobilePopModelResult" class="konkaMobilePopModel" extends="konkaMobilePopModelResultForList">
		<result column="MODEL_NAME" property="map.model_name" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="sf-konkaMobilePopModel">
		<isNotEmpty prepend=" and " property="id">ID = #id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="model_id">MODEL_ID = #model_id:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="user_id">USER_ID = #user_id:DECIMAL#</isNotEmpty>
	</sql>

	<select id="selectKonkaMobilePopModel" resultMap="konkaMobilePopModelResult" parameterClass="konkaMobilePopModel" cacheModel="oneDayCache">
		select *,a.md_name as MODEL_NAME from KONKA_MOBILE_POP_MODEL left join konka_pe_pd_model a on model_id = a.pd_id where 1 = 1
		<include refid="sf-konkaMobilePopModel" />
	</select>

	<select id="selectKonkaMobilePopModelList" resultMap="konkaMobilePopModelResultForList" parameterClass="konkaMobilePopModel" cacheModel="oneDayCache">
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_MOBILE_POP_MODEL where 1 = 1
		<include refid="sf-konkaMobilePopModel" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

	<select id="selectKonkaMobilePopModelCount" resultClass="long" parameterClass="konkaMobilePopModel" cacheModel="oneDayCache">
		select count(*) from KONKA_MOBILE_POP_MODEL where 1 = 1
		<include refid="sf-konkaMobilePopModel" />
	</select>

	<select id="selectKonkaMobilePopModelPaginatedList" resultMap="konkaMobilePopModelResult" parameterClass="konkaMobilePopModel" cacheModel="oneDayCache">
		<![CDATA[ select * from ( select t_.*, rownum rn_ from ( ]]>
		select *,a.md_name as MODEL_NAME from KONKA_MOBILE_POP_MODEL left join konka_pe_pd_model a on model_id = a.pd_id where 1 = 1
		<include refid="sf-konkaMobilePopModel" />
		<!-- order by ID asc -->
		<![CDATA[ ) t_ where rownum <= (#row.first# + #row.count#)) where rn_ >= (#row.first# + 1) ]]>
	</select>

	<insert id="insertKonkaMobilePopModel" parameterClass="konkaMobilePopModel">
		<selectKey resultClass="long" keyProperty="id">select KONKA_MOBILE_REPORT.nextval as id from dual </selectKey>
		<![CDATA[insert into KONKA_MOBILE_POP_MODEL (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">ID</isNotNull>	
			<isNotNull prepend="," property="model_id">MODEL_ID</isNotNull>	
			<isNotNull prepend="," property="user_id">USER_ID</isNotNull>	
		</dynamic>
		<![CDATA[) values (]]>
		<dynamic prepend=" ">
			<isNotNull prepend="," property="id">#id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="model_id">#model_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_id">#user_id:DECIMAL#</isNotNull>
		</dynamic>
		<![CDATA[)]]>
	</insert>

	<update id="updateKonkaMobilePopModel" parameterClass="KonkaMobilePopModel">
		update KONKA_MOBILE_POP_MODEL
		<dynamic prepend="set">
			<isNotNull prepend="," property="id">ID = #id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="model_id">MODEL_ID = #model_id:DECIMAL#</isNotNull>
			<isNotNull prepend="," property="user_id">USER_ID = #user_id:DECIMAL#</isNotNull>
		</dynamic>
		where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</update>

	<delete id="deleteKonkaMobilePopModel" parameterClass="KonkaMobilePopModel">
		delete from KONKA_MOBILE_POP_MODEL where
		<isNotEmpty prepend=" " property="id">ID = #id#</isNotEmpty>
		<isEmpty prepend=" " property="id">
			<isNotEmpty prepend=" " property="map.pks">
				ID in
				<iterate close=")" open="(" conjunction="," property="map.pks">#map.pks[]#</iterate>
			</isNotEmpty>
		</isEmpty>
	</delete>

</sqlMap>