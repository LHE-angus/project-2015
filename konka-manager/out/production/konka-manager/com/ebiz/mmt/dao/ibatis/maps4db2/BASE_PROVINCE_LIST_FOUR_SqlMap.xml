		>
		SELECT t.* FROM KONKA_BASE_PROVINCE_LIST_FOUR t
		 WHERE 1 = 1 and t.P_LEVEL = 1 
		  <isNotEmpty prepend="  " property="map.pindex">
		  	START WITH t.P_INDEX = #map.pindex#
		CONNECT BY PRIOR t.par_index = t.P_INDEX
      	 </isNotEmpty>
	</select>
	
