		<isNotEmpty prepend=" and " property="is_west">aa.IS_WEST = #is_west:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_sort">aa.ORDER_SORT = #order_sort:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">aa.ADD_DATE = #add_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="del_mark">aa.DEL_MARK = #del_mark:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="del_date">aa.DEL_DATE = #del_date:DATETIME#</isNotEmpty>
		<isNotEmpty prepend=" and " property="full_name">aa.FULL_NAME = #full_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.many_p_index">aa.P_INDEX in ($map.many_p_index$)</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.p_index_in">
				aa.p_index in
				<iterate close=")" open="(" conjunction="," property="map.p_index_in">#map.p_index_in[]#</iterate>
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.p_name_2">(aa.P_NAME = #map.p_name_2# or aa.P_NAME like '%'||#map.p_name_2#||'%')</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.p_index_join">
			exists ( select distinct(t.p_index) from base_province_list t where aa.p_index = t.p_index start with t.p_index in ($map.p_index_join$) connect by prior t.par_index = t.p_index 
			 union select distinct(t.p_index) from base_province_list t where aa.p_index = t.p_index start with t.p_index in ($map.p_index_join$) connect by prior t.p_index = t.par_index)
		</isNotEmpty>
		
        <!-- 作战地图.市的区，县获取 ChengBing -->
		<isNotEmpty prepend=" and " property="map.p_index_child">
			aa.P_INDEX like #map.p_index_child:VARCHAR# || '%'
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.p_level_child">
			aa.P_LEVEL in ($map.p_level_child$)
		</isNotEmpty>
		order by aa.P_INDEX asc
		<isNotEmpty property="row.count">
			<![CDATA[ ) where rownum <= #row.count# ]]>
		</isNotEmpty>
	</select>

