	<select id="selectBaseProvinceListFourList" resultMap="baseProvinceListFourResultForList" parameterClass="baseProvinceListFour" >
		<isNotEmpty property="row.count">
			<![CDATA[ select * from ( ]]>
		</isNotEmpty>
		select * from KONKA_BASE_PROVINCE_LIST_FOUR aa where 1 = 1
		<isNotEmpty prepend=" and " property="p_index">aa.P_INDEX = #p_index:DECIMAL#</isNotEmpty>
		<isNotEmpty prepend=" and " property="p_name">aa.P_NAME = #p_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="s_name">aa.S_NAME = #s_name:VARCHAR#</isNotEmpty>
