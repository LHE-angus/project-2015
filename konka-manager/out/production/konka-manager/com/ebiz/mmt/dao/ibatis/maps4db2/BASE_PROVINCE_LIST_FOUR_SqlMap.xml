	<!-- Pan,Gang 根据省市县去对应的pindex -->
	<select id="selectBaseProvinceListFourByCityNameList" resultMap="baseProvinceListFourResultForList" parameterClass="baseProvinceListFour">
		SELECT t.*
  FROM KONKA_BASE_PROVINCE_LIST t
 WHERE    1 = 1 AND t.DEL_MARK = 0
       <isNotEmpty prepend=" and " property="map.country"> t.P_NAME = #map.country#</isNotEmpty> 
       <isNotEmpty prepend=" and " property="map.city">
        (SELECT b.P_NAME
              FROM KONKA_BASE_PROVINCE_LIST_FOUR b
             WHERE 1 = 1 AND b.P_LEVEL = 2
            START WITH b.P_INDEX = t.P_INDEX
            CONNECT BY PRIOR b.par_index = b.P_INDEX) = #map.city#
        </isNotEmpty>
        <isNotEmpty prepend=" and " property="map.province">   
        (SELECT b.P_NAME
              FROM KONKA_BASE_PROVINCE_LIST_FOUR b
             WHERE 1 = 1 AND b.P_LEVEL = 1
            START WITH b.P_INDEX = t.P_INDEX
            CONNECT BY PRIOR b.par_index = b.P_INDEX) = #map.province#
        </isNotEmpty>    
	</select>
	
	<!-- Pan,Gang 取区域仓库关联的省份 -->
	<select id="selectBaseProvinceListFourForProvinceInList" resultMap="baseProvinceListFourResultForList" parameterClass="baseProvinceListFour"
		>
		SELECT t.* FROM KONKA_BASE_PROVINCE_LIST_FOUR t
		 WHERE P_LEVEL = 1 
      	 <isNotEmpty prepend=" and " property="map.store_id">
				 p_index in	 (SELECT  t.PINDEX_ID
					 FROM   ec_base_store a 
			      	 left JOIN   EC_STORE_AREA t
			      	 ON t.STORE_ID = a.STORE_ID  where  1=1 and a.STORE_TYPE = 1 
	      	 		  and a.STORE_ID=#map.store_id#
      	 </isNotEmpty>
      	 <isNull prepend=" and " property="map.store_id">
      	 		p_index not in (SELECT p_index  FROM KONKA_BASE_PROVINCE_LIST_FOUR
      	 </isNull>
	      	  )
	</select>
	
