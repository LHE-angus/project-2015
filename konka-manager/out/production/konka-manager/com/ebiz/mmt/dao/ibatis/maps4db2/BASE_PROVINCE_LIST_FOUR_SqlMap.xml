	<select id="selectBaseProvinceListFourSonTreeList" resultMap="baseProvinceListFourResultForList" parameterClass="baseProvinceListFour"
		>
		select * from KONKA_BASE_PROVINCE_LIST_FOUR r 
		where r.del_mark = 0
		<isNotEmpty prepend=" and " property="map.p_level"><![CDATA[  P_LEVEL > #map.p_level# ]]></isNotEmpty>
		<isNotEmpty prepend=" and " property="map.level"><![CDATA[  P_LEVEL = #map.level# ]]></isNotEmpty>
		start with p_index = $p_index$ and del_mark = 0 connect by prior p_index = par_index
		order by level asc,p_index asc
	</select>
	
